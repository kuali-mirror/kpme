        <field-descriptor name="workArea"               column="WORK_AREA"                  jdbc-type="BIGINT"      nullable="false" primarykey="false" />
        <field-descriptor name="task"                   column="TASK"                       jdbc-type="BIGINT"      nullable="false" primarykey="false" />
