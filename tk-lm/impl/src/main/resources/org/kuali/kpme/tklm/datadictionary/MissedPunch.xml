        <property name="resultFields">
            <list>
		        <bean parent="Uif-DataField" p:propertyName="personName" />
		        <bean parent="Uif-DataField" p:propertyName="timesheetDocumentId" />
		        <bean parent="Uif-DataField" p:propertyName="assignmentValue" />
