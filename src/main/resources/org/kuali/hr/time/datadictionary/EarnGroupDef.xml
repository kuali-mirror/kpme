<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean name="EarnGroupDef" parent="EarnGroupDef-parentBean" />

	<bean id="EarnGroupDef-parentBean" abstract="true"
		parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.hr.time.earncode.EarnGroupDef" />
		<property name="titleAttribute" value="tkEarnGroupDefId" />
		<property name="objectLabel" value="earnGroupDef" />
		<property name="lookupDefinition">
			<ref bean="EarnGroupDef-lookupDefinition" />
		</property>
		<property name="attributes">
			<list>
				<ref bean="EarnGroupDef-tkEarnGroupDefId" />
				<ref bean="EarnGroupDef-earnCode" />
			</list>
		</property>
	</bean>

	<bean id="EarnGroupDef-tkEarnGroupDefId" parent="EarnGroupDef-tkEarnGroupDefId-parentBean" />
	<bean id="EarnGroupDef-tkEarnGroupDefId-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Id" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="tkEarnGroupDefId" />
		<property name="label" value="Earn Group Def Id" />
	</bean>

	<bean id="EarnGroupDef-earnCode" parent="EarnGroupDef-earnCode-parentBean" />
	<bean id="EarnGroupDef-earnCode-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Earn Code" />
		<property name="maxLength" value="30" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="30" />
		</property>
		<property name="name" value="earnCode" />
		<property name="label" value="Earn Code" />
	</bean>

	<!--  Inquiry Definitions  -->


	<bean id="EarnGroupDef-lookupDefinition" parent="EarnGroupDef-lookupDefinition-parentBean" />
	<bean id="EarnGroupDef-lookupDefinition-parentBean" abstract="true"
		parent="LookupDefinition">
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="tkEarnGroupDefId" />
				<bean parent="FieldDefinition" p:attributeName="earnCode" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="tkEarnGroupDefId" />
				<bean parent="FieldDefinition" p:attributeName="earnCode" />
			</list>
		</property>
		<property name="title" value="Earn Group Definition Lookup" />
	</bean>

</beans>