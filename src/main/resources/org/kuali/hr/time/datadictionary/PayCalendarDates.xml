<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">


  <bean id="PayCalendarDates" parent="PayCalendarDates-parentBean"/>
  <bean id="PayCalendarDates-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.hr.time.paycalendar.PayCalendarDates"/>
    <property name="titleAttribute" value="payCalendarId"/>
    <property name="objectLabel" value="PayCalendarDates"/>
    <property name="lookupDefinition">
      <ref bean="PayCalendarDates-lookupDefinition"/>
    </property>
    <property name="attributes">
      <list>
        <ref bean="PayCalendarDates-payCalendarId"/>
        <ref bean="PayCalendarDates-beginPeriodDate"/>
        <ref bean="PayCalendarDates-beginPeriodTime"/>
        <ref bean="PayCalendarDates-endPeriodDate"/>
        <ref bean="PayCalendarDates-endPeriodTime"/>
      </list>
    </property>    
  </bean>

  <!-- Attribute Definitions -->
  
  <bean id="PayCalendarDates-payCalendarId" parent="PayCalendarDates-payCalendarId-parentBean"/>
  <bean id="PayCalendarDates-payCalendarId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Id"/>
    <property name="maxLength" value="20"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="20"/>
    </property>
    <property name="name" value="payCalendarId"/>
    <property name="label" value="Pay Calendar Id"/>
  </bean>
  
    
  <bean id="PayCalendarDates-beginPeriodDate" parent="PayCalendarDates-beginPeriodDate-parentBean"/>
  <bean id="PayCalendarDates-beginPeriodDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Begin Period Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlDateFormatter"/>
    <property name="name" value="beginPeriodDate"/>
    <property name="label" value="Begin Period Date"/>
  </bean>

  <bean id="PayCalendarDates-beginPeriodTime" parent="PayCalendarDates-beginPeriodTime-parentBean"/>
  <bean id="PayCalendarDates-beginPeriodTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Begin Period Time"/>
    <property name="maxLength" value="10"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>    
    <property name="validationPattern">
 		<bean parent="RegexValidationPattern" p:pattern="[0-9]{2}:[0-9]{2}\s*[Aa|Pp][Mm]" p:validationErrorMessageKey="MISSING"/>
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlTimeFormatter"/>
    <property name="name" value="beginPeriodTime"/>
    <property name="label" value="Begin Period Time"/>
  </bean>

  <bean id="PayCalendarDates-endPeriodDate" parent="PayCalendarDates-endPeriodDate-parentBean"/>
  <bean id="PayCalendarDates-endPeriodDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="End Period Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlDateFormatter"/>
    <property name="name" value="endPeriodDate"/>
    <property name="label" value="End Period Date"/>
  </bean>

  <bean id="PayCalendarDates-endPeriodTime" parent="PayCalendarDates-endPeriodTime-parentBean"/>
  <bean id="PayCalendarDates-endPeriodTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="End Period Time"/>
    <property name="maxLength" value="10"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>      
    <property name="validationPattern">
 		<bean parent="RegexValidationPattern" p:pattern="[0-9]{2}:[0-9]{2}\s*[Aa|Pp][Mm]" p:validationErrorMessageKey="MISSING"/>
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlTimeFormatter"/>
    <property name="name" value="endPeriodTime"/>
    <property name="label" value="End Period Time"/>
  </bean>


  <bean id="PayCalendarDates-lookupDefinition" parent="PayCalendarDates-lookupDefinition-parentBean"/>
  <bean id="PayCalendarDates-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="payCalendarId"/>
        <bean parent="FieldDefinition" p:attributeName="beginPeriodDate"/>
        <bean parent="FieldDefinition" p:attributeName="beginPeriodTime"/>
        <bean parent="FieldDefinition" p:attributeName="endPeriodDate"/>
        <bean parent="FieldDefinition" p:attributeName="endPeriodTime"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="payCalendarId"/>
        <bean parent="FieldDefinition" p:attributeName="beginPeriodDate"/>
        <bean parent="FieldDefinition" p:attributeName="beginPeriodTime"/>
        <bean parent="FieldDefinition" p:attributeName="endPeriodDate"/>
        <bean parent="FieldDefinition" p:attributeName="endPeriodTime"/>
      </list>
    </property>
    <property name="title" value="Pay Calendar Dates Lookup"/>
  </bean>
   
</beans>
