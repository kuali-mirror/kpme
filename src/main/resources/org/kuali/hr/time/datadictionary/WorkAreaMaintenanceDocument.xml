<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="WorkAreaMaintenanceDocument" parent="WorkAreaMaintenanceDocument-parentBean"/>
  <bean id="WorkAreaMaintenanceDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="documentTypeName" value="WorkAreaMaintenanceDocument"/>
    <property name="documentClass" value="org.kuali.hr.time.workarea.WorkAreaMaintenanceDocument"/>
    <property name="documentAuthorizerClass" value="org.kuali.hr.time.workarea.web.WorkAreaMaintenanceDocumentAuthorizer"/>
    <property name="attributes">
      <list>
        <ref bean="WorkAreaMaintenanceDocument-workAreaId"/>
        <ref bean="WorkAreaMaintenanceDocument-deptId"/>
        <ref bean="WorkAreaMaintenanceDocument-active"/>
        <ref bean="WorkAreaMaintenanceDocument-overtimePreference"/>
        <ref bean="WorkAreaMaintenanceDocument-description"/>
        <ref bean="WorkAreaMaintenanceDocument-adminDescr"/>
      </list>
    </property> 
  </bean>
 

  <bean id="WorkAreaMaintenanceDocument-workAreaId" parent="WorkArea-workAreaId" abstract="false" p:name="workArea.workAreaId" p:required="true"/>
  <bean id="WorkAreaMaintenanceDocument-deptId" parent="WorkArea-deptId" abstract="false" p:name="workArea.deptId" p:required="true"/>
  <bean id="WorkAreaMaintenanceDocument-active" parent="WorkArea-active" abstract="false" p:name="workArea.active" p:required="true"/>
  <bean id="WorkAreaMaintenanceDocument-description" parent="WorkArea-description" abstract="false" p:name="workArea.description" p:required="true"/>
  <bean id="WorkAreaMaintenanceDocument-overtimePreference" parent="WorkArea-overtimePreference" abstract="false" p:name="workArea.overtimePreference" p:required="true"/>
  <bean id="WorkAreaMaintenanceDocument-adminDescr" parent="WorkArea-adminDescr" abstract="false" p:name="workArea.adminDescr" p:required="true"/>
  
  
</beans>
