<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean name="PrincipalPayType" parent="PrincipalPayType-parentBean" />

	<bean id="PrincipalPayType-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.hr.time.paytype.PrincipalPayType" />
		<property name="titleAttribute" value="principalPayTypeId" />
		<property name="objectLabel" value="Pay Type" />
		<property name="lookupDefinition">
			<ref bean="PrincipalPayType-lookupDefinition" />
		</property>
		<property name="attributes">
			<list>
				<ref bean="PrincipalPayType-principalPayTypeId" />
				<ref bean="PrincipalPayType-principalId" />
				<ref bean="PrincipalPayType-hrPayType" />
			</list>
		</property>
		<property name="relationships">
			<bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="payType"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="hrPayType" p:targetName="payType"/>
                </list>
              </property>
            </bean>

		</property>
	</bean>

	<bean id="PrincipalPayType-principalPayTypeId" parent="PrincipalPayType-principalPayTypeId-parentBean" />
	<bean id="PrincipalPayType-principalPayTypeId-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="shortLabel" value="Id" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="principalPayTypeId" />
		<property name="label" value="Principal Pay Type Id" />
	</bean>

	<bean id="PrincipalPayType-principalId" parent="PrincipalPayType-principalId-parentBean" />
	<bean id="PrincipalPayType-principalId-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="shortLabel" value="Principal" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="principalId" />
		<property name="label" value="PrincipalId" />
	</bean>
	
	<bean id="PrincipalPayType-hrPayType" parent="PrincipalPayType-hrPayType-parentBean" />
	<bean id="PrincipalPayType-hrPayType-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Pay Type" />
		<property name="maxLength" value="5" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="5" />
		</property>
		<property name="name" value="hrPayType" />
		<property name="label" value="Pay Type Id" />
	</bean>

	<!--  Inquiry Definitions  -->


	<bean id="PrincipalPayType-lookupDefinition" parent="PrincipalPayType-lookupDefinition-parentBean" />
	<bean id="PrincipalPayType-lookupDefinition-parentBean" abstract="true"
		parent="LookupDefinition">
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="principalPayTypeId" />
				<bean parent="FieldDefinition" p:attributeName="principalId" />
				<bean parent="FieldDefinition" p:attributeName="hrPayType" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="principalPayTypeId" />
				<bean parent="FieldDefinition" p:attributeName="principalId" />
				<bean parent="FieldDefinition" p:attributeName="hrPayType" />
			</list>
		</property>
		<property name="title" value="Principal Pay Type Lookup" />
	</bean>

</beans>