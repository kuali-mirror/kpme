<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">


  <bean id="TimeSheetInitiate" parent="TimeSheetInitiate-parentBean"/>
  <bean id="TimeSheetInitiate-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.hr.time.timesheet.TimeSheetInitiate"/>
    <property name="titleAttribute" value="tkTimeSheetInitId"/>
    <property name="objectLabel" value="TimeSheetInitiate"/>
    <property name="lookupDefinition">
      	<ref bean="TimeSheetInitiate-lookupDefinition"/>
    </property>
    <property name="inquiryDefinition">
		<ref bean="TimeSheetInitiate-inquiryDefinition" />
	</property>
    <property name="attributes">
      <list>
      	<ref bean="TimeSheetInitiate-tkTimeSheetInitId"/>
        <ref bean="TimeSheetInitiate-principalId"/>
        <ref bean="TimeSheetInitiate-hrPyCalendarEntriesId"/>
        <ref bean="TimeSheetInitiate-pyCalendarGroup"/>
        <ref bean="TimeSheetInitiate-documentId"/>
        <ref bean="TimeSheetInitiate-beginAndEndDateTime"/>
        
      </list>
    </property>
    <property name="relationships">
  		<list>
            <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="principal"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="principalId" p:targetName="principalId"/>
                </list>
              </property>
            </bean>
            <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="payCalendarObj"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="pyCalendarGroup" p:targetName="pyCalendarGroup"/>
                </list>
              </property>
            </bean>
            <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="payCalendarEntriesObj"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="hrPyCalendarEntriesId" p:targetName="hrPyCalendarEntriesId"/>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="pyCalendarGroup" p:targetName="pyCalendarGroup"/>
                </list>
              </property>
            </bean>
         </list>
    </property>
    
  </bean>
  
   <!-- Attribute Definitions -->

	<bean id="TimeSheetInitiate-tkTimeSheetInitId" parent="TimeSheetInitiate-tkTimeSheetInitId-parentBean" />
	<bean id="TimeSheetInitiate-tkTimeSheetInitId-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="shortLabel" value="TimeSheetInitiate Id" />
		<property name="maxLength" value="60" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="60" />
		</property>
		<property name="name" value="tkTimeSheetInitId" />
		<property name="label" value="Time Sheet Initiate Id" />
	</bean>

	<bean id="TimeSheetInitiate-principalId" parent="TimeSheetInitiate-principalId-parentBean" />
	<bean id="TimeSheetInitiate-principalId-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="shortLabel" value="Principal Id" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="principalId" />
		<property name="label" value="Principal Id" />
	</bean>
	
	
	<bean id="TimeSheetInitiate-beginAndEndDateTime" parent="TimeSheetInitiate-beginAndEndDateTime-parentBean"/>
  	<bean id="TimeSheetInitiate-beginAndEndDateTime-parentBean" abstract="true" parent="AttributeDefinition">
    	<property name="shortLabel" value="Date Time"/>
    	<property name="maxLength" value="80"/>
	    <property name="control">
      		<bean parent="LookupReadonlyControlDefinition" p:size="80" />
    	</property>
    	<property name="name" value="beginAndEndDateTime"/>
    	<property name="label" value="Begin And End Date Time"/>
  	</bean>
	

	  <bean id="TimeSheetInitiate-hrPyCalendarEntriesId" parent="TimeSheetInitiate-hrPyCalendarEntriesId-parentBean"/>
  <bean id="TimeSheetInitiate-hrPyCalendarEntriesId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Pay Calendar Entry Id"/>
    <property name="maxLength" value="20"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="20"/>
    </property>
    <property name="name" value="hrPyCalendarEntriesId"/>
    <property name="label" value="Pay Calendar Entry Id"/>
  </bean>	
	
	<bean id="TimeSheetInitiate-pyCalendarGroup" parent="TimeSheetInitiate-pyCalendarGroup-parentBean" />
	<bean id="TimeSheetInitiate-pyCalendarGroup-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="shortLabel" value="Pay Calendar" />
		<property name="maxLength" value="30" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="yes" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="30" />
		</property>
		<property name="name" value="pyCalendarGroup" />
		<property name="label" value="Pay Calendar" />
	</bean> 
	 
	  <bean id="TimeSheetInitiate-documentId" parent="TimeSheetInitiate-documentId-parentBean"/>
	  <bean id="TimeSheetInitiate-documentId-parentBean" abstract="true" parent="AttributeDefinition">
	    <property name="shortLabel" value="Document Id"/>
	    <property name="maxLength" value="14"/>
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern"/>
		</property>
	    <property name="control">
	      <bean parent="TextControlDefinition" p:size="14"/>
	    </property>
	    <property name="name" value="documentId"/>
	    <property name="label" value="Document Id"/>
	  </bean>
	 
  <!-- Lookup Definition: -->

  <bean id="TimeSheetInitiate-lookupDefinition" parent="TimeSheetInitiate-lookupDefinition-parentBean"/>
  <bean id="TimeSheetInitiate-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
  	<property name="lookupableID" value="timeSheetInitiateLookupable" />
  	 <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields">
      <list>
     	<bean parent="FieldDefinition" p:attributeName="tkTimeSheetInitId"/>
        <bean parent="FieldDefinition" p:attributeName="principalId"/>
        <bean parent="FieldDefinition" p:attributeName="hrPyCalendarEntriesId"/>
        <bean parent="FieldDefinition" p:attributeName="pyCalendarGroup"/>
       </list>
    </property>
    <property name="resultFields">
      <list>
     	<bean parent="FieldDefinition" p:attributeName="tkTimeSheetInitId"/>
        <bean parent="FieldDefinition" p:attributeName="principalId"/>
        <bean parent="FieldDefinition" p:attributeName="hrPyCalendarEntriesId"/>
        <bean parent="FieldDefinition" p:attributeName="pyCalendarGroup"/>
        <bean parent="FieldDefinition" p:attributeName="documentId"/>
      </list>
    </property>
    <property name="title" value="Time Sheet Initiate Lookup"/>
  </bean>
  
  <!-- Inquiry Definition -->
  
	<bean id="TimeSheetInitiate-inquiryDefinition" parent="TimeSheetInitiate-inquiryDefinition-parentBean" />
	<bean id="TimeSheetInitiate-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
					        <bean parent="FieldDefinition" p:attributeName="principalId"/>
					        <bean parent="FieldDefinition" p:attributeName="pyCalendarGroup"/>
					        <bean parent="FieldDefinition" p:attributeName="hrPyCalendarEntriesId"/>
					        <bean parent="FieldDefinition" p:attributeName="beginAndEndDateTime"/>
					        <bean parent="FieldDefinition" p:attributeName="documentId"/>
						</list>
					</property>
					<property name="title" value="Time Sheet Initiate" />
				</bean>
			</list>
		</property>
		<property name="title" value="Time Sheet Initiate Inquiry" />
	</bean>
  

</beans>

