<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean name="DepartmentEarnCode" parent="DepartmentEarnCode-parentBean" />

	<bean id="DepartmentEarnCode-parentBean" abstract="true"
		parent="BusinessObjectEntry">
		<property name="businessObjectClass"
			value="org.kuali.hr.time.dept.earncode.DepartmentEarnCode" />
		<property name="titleAttribute" value="tkDeptEarnCodeId" />
		<property name="objectLabel" value="Department Earn Code" />
		<property name="inquiryDefinition">
			<ref bean="DepartmentEarnCode-inquiryDefinition" />
		</property>
		<property name="lookupDefinition">
			<ref bean="DepartmentEarnCode-lookupDefinition" />
		</property>
		<property name="attributes">
			<list>
				<ref bean="DepartmentEarnCode-dept" />
				<ref bean="DepartmentEarnCode-tkSalGroup" />
				<ref bean="DepartmentEarnCode-earnCode" />
				<ref bean="DepartmentEarnCode-location" />
				<ref bean="DepartmentEarnCode-effectiveDate" />
				<ref bean="DepartmentEarnCode-timestamp" />
				<ref bean="DepartmentEarnCode-employee" />
				<ref bean="DepartmentEarnCode-approver" />
				<ref bean="DepartmentEarnCode-org_admin" />
				<ref bean="DepartmentEarnCode-history" />
				<ref bean="DepartmentEarnCode-active" />
			</list>
		</property>
		  
		<property name="relationships">
			<list>
				<bean parent="RelationshipDefinition">
					<property name="objectAttributeName" value="departmentObj" />
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition" p:sourceName="dept"
								p:targetName="dept" />
						</list>
					</property>
	              <property name="supportAttributes">
	                <list>
	                  <bean parent="SupportAttributeDefinition" p:identifier="false" p:sourceName="active" p:targetName="tkDeptId"/>
	                </list>
	              </property>
				</bean>
				
				<bean parent="RelationshipDefinition">
					<property name="objectAttributeName" value="salGroupObj" />
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition" p:sourceName="tkSalGroup"
								p:targetName="tkSalGroup" />
						</list>
					</property>
	              <property name="supportAttributes">
	                <list>
	                  <bean parent="SupportAttributeDefinition" p:identifier="false" p:sourceName="active" p:targetName="tkSalGroupId"/>
	                </list>
	              </property>
				</bean>
				
				<bean parent="RelationshipDefinition">
					<property name="objectAttributeName" value="earnCodeObj" />
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition" p:sourceName="earnCode"
								p:targetName="earnCode" />
						</list>
					</property>
		              <property name="supportAttributes">
		                <list>
		                  <bean parent="SupportAttributeDefinition" p:identifier="false" p:sourceName="active" p:targetName="tkEarnCodeId"/>
		                </list>
		              </property>
				</bean>

				<bean parent="RelationshipDefinition">
					<property name="objectAttributeName" value="locationObj" />
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition" p:sourceName="location"
								p:targetName="location" />
						</list>
					</property>
	              <property name="supportAttributes">
	                <list>
	                  <bean parent="SupportAttributeDefinition" p:identifier="false" p:sourceName="active" p:targetName="hrLocationId"/>
	                </list>
	              </property>
				</bean>
			</list>
		</property>
		
	</bean>

	<!-- Attributes  -->

	<!-- deptId -->
	<bean id="DepartmentEarnCode-dept" parent="DepartmentEarnCode-dept-parentBean" />
	<bean id="DepartmentEarnCode-dept-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Department" />
		<property name="maxLength" value="21" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="21" />
		</property>
		<property name="name" value="dept" />
		<property name="label" value="Department" />
	</bean>

	<!-- salGroup -->
	<bean id="DepartmentEarnCode-tkSalGroup" parent="DepartmentEarnCode-tkSalGroup-parentBean" />
	<bean id="DepartmentEarnCode-tkSalGroup-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Sal. Group" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="tkSalGroup" />
		<property name="label" value="Sal. Group" />
	</bean>
	<!-- earnCode -->
	<bean id="DepartmentEarnCode-earnCode" parent="DepartmentEarnCode-earnCode-parentBean" />
	<bean id="DepartmentEarnCode-earnCode-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Earn Code" />
		<property name="maxLength" value="3" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="3" />
		</property>
		<property name="name" value="earnCode" />
		<property name="label" value="Earn Code" />
	</bean>

    <bean id="DepartmentEarnCode-location" parent="DepartmentEarnCode-location-parentBean" />
	<bean id="DepartmentEarnCode-location-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Location" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="location" />
		<property name="label" value="Location" />
	</bean>

  <bean id="DepartmentEarnCode-effectiveDate" parent="DepartmentEarnCode-effectiveDate-parentBean"/>
  <bean id="DepartmentEarnCode-effectiveDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Effective Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="12" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter"/>
    <property name="name" value="effectiveDate"/>
    <property name="label" value="Effective Date"/>
  </bean>
  	
	<!-- Employee -->
	<bean id="DepartmentEarnCode-employee" parent="DepartmentEarnCode-employee-parentBean" />
	<bean id="DepartmentEarnCode-employee-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Employee" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="name" value="employee" />
		<property name="label" value="Employee" />
	</bean>

	<!-- Approver -->
	<bean id="DepartmentEarnCode-approver" parent="DepartmentEarnCode-approver-parentBean" />
	<bean id="DepartmentEarnCode-approver-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Approver" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="name" value="approver" />
		<property name="label" value="Approver" />
	</bean>

	<!-- org_admin -->
	<bean id="DepartmentEarnCode-org_admin" parent="DepartmentEarnCode-org_admin-parentBean" />
	<bean id="DepartmentEarnCode-org_admin-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Org. admin" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="name" value="org_admin" />
		<property name="label" value="Org. Admin" />
	</bean>
	<!-- timeStamp -->
	<bean id="DepartmentEarnCode-timestamp" parent="DepartmentEarnCode-timestamp-parentBean" />
	<bean id="DepartmentEarnCode-timestamp-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Timestamp" />
		<property name="maxLength" value="15" />
		<property name="control">
			<bean parent="LookupReadonlyControlDefinition" p:size="15" />
		</property>
		<property name="formatterClass"
			value="org.kuali.rice.kns.web.format.TimestampAMPMFormatter" />
		<property name="name" value="timestamp" />
		<property name="label" value="Time Stamp" />
	</bean>
	<!-- history -->
	<bean id="DepartmentEarnCode-history" parent="DepartmentEarnCode-history-parentBean" />
	<bean id="DepartmentEarnCode-history-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Show History" />
		<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.time.util.TkHistoryKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="history" />
		<property name="label" value="Show History" />
	</bean>
	
	<bean id="DepartmentEarnCode-active" parent="DepartmentEarnCode-active-parentBean"/>
    <bean id="DepartmentEarnCode-active-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Active"/>
    <property name="control">
      <bean parent="CheckboxControlDefinition"/>
    </property>
    <property name="name" value="active"/>
    <property name="label" value="Active"/>
    <property name="formatterClass" value="org.kuali.rice.kns.web.format.BooleanFormatter"/>
  </bean>

 	<!--  Inquiry Definitions  -->
  
  	<bean id="DepartmentEarnCode-inquiryDefinition" parent="DepartmentEarnCode-inquiryDefinition-parentBean" />

	<bean id="DepartmentEarnCode-inquiryDefinition-parentBean" abstract="true"
		parent="InquiryDefinition">
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" p:attributeName="effectiveDate" />
							<bean parent="FieldDefinition" p:attributeName="dept" />
							<bean parent="FieldDefinition" p:attributeName="tkSalGroup" />
							<bean parent="FieldDefinition" p:attributeName="earnCode" />
							<bean parent="FieldDefinition" p:attributeName="location" />
							<bean parent="FieldDefinition" p:attributeName="employee" />
							<bean parent="FieldDefinition" p:attributeName="approver" />
							<bean parent="FieldDefinition" p:attributeName="org_admin" />
							<bean parent="FieldDefinition" p:attributeName="active" />
						</list>
					</property>
					<property name="title" value="DepartmentEarnCode" />
				</bean>
			</list>
		</property>
		<property name="title" value="DepartmentEarnCode Inquiry" />
	</bean>

	<!-- Look up Definitions  -->

	<bean id="DepartmentEarnCode-lookupDefinition" parent="DepartmentEarnCode-lookupDefinition-parentBean" />
	<bean id="DepartmentEarnCode-lookupDefinition-parentBean"
		abstract="true" parent="LookupDefinition">
		<property name="lookupableID" value="departmentEarnCodeLookupable"></property>
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="dept" />
				<bean parent="FieldDefinition" p:attributeName="tkSalGroup" />
				<bean parent="FieldDefinition" p:attributeName="earnCode" />
				<bean parent="FieldDefinition" p:attributeName="location" />
				<bean parent="FieldDefinition" p:attributeName="effectiveDate" />
				<bean parent="FieldDefinition" p:attributeName="active" />
				<bean parent="FieldDefinition" p:attributeName="history" p:defaultValue="N"/>
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="dept" />
				<bean parent="FieldDefinition" p:attributeName="tkSalGroup" />
				<bean parent="FieldDefinition" p:attributeName="earnCode" />
				<bean parent="FieldDefinition" p:attributeName="location" />
				<bean parent="FieldDefinition" p:attributeName="employee" />
				<bean parent="FieldDefinition" p:attributeName="approver" />
				<bean parent="FieldDefinition" p:attributeName="org_admin" />
				<bean parent="FieldDefinition" p:attributeName="effectiveDate" />
				<bean parent="FieldDefinition" p:attributeName="timestamp" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
		<property name="title" value="Department Earn Code Lookup" />
	</bean>
</beans>