<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:dd="http://rice.kuali.org/dd"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
                           http://rice.kuali.org/dd
                           http://rice.kuali.org/dd/dd.xsd">

  <bean id="PayCalendarEntriesMaintenanceDocument" parent="PayCalendarEntriesMaintenanceDocument-parentBean"/>

  <bean id="PayCalendarEntriesMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry" 
  		p:businessObjectClass="org.kuali.hr.time.paycalendar.PayCalendarEntries"
  		p:businessRulesClass="org.kuali.hr.time.paycalendar.validation.PayCalendarEntriesRule"
   		p:maintainableClass="org.kuali.hr.time.paycalendar.PayCalendarEntryMaintainableImpl">
    <property name="maintainableSections">
      <list>
        <ref bean="PayCalendarEntriesMaintenanceDocument-PayCalendarEntriesMaintenance"/>
      </list>
    </property>
	<property name="lockingKeys">
			<list>
				<value>payCalendarEntriesId</value>
			</list>
	</property>    
    <property name="documentTypeName" value="PayCalendarEntriesDocumentType"/>
  </bean>

  <!-- Maintenance Section Definitions -->

  <bean id="PayCalendarEntriesMaintenanceDocument-PayCalendarEntriesMaintenance" parent="PayCalendarEntriesMaintenanceDocument-PayCalendarEntriesMaintenance-parentBean"/>
  <bean id="PayCalendarEntriesMaintenanceDocument-PayCalendarEntriesMaintenance-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="title" value="Pay Calendar Dates Maintenance"/>
    <property name="maintainableItems">
      <list>
     	<bean parent="MaintainableFieldDefinition" p:name="calendarGroup" p:required="true"/>
     	<bean parent="MaintainableFieldDefinition" p:name="payCalendarId"  p:noLookup="true" p:lookupReadOnly="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="beginPeriodDate" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="beginPeriodTime" p:required="true"/>
      	<bean parent="MaintainableFieldDefinition" p:name="endPeriodDate"    p:required="true"/>
      	<bean parent="MaintainableFieldDefinition" p:name="endPeriodTime"    p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="batchInitiateDate"   p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="batchInitiateTime"   p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="batchEndPayPeriodDate"   p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="batchEndPayPeriodTime"   p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="batchEmployeeApprovalDate"   p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="batchEmployeeApprovalTime"   p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="batchSupervisorApprovalDate"   p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="batchSupervisorApprovalTime"   p:required="true"/>         
      </list>
    </property>
  </bean>

</beans>
