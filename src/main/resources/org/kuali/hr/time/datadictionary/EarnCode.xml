<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="EarnCode" parent="EarnCode-parentBean" />
  <bean id="EarnCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.hr.time.earncode.EarnCode" />
    <property name="inquiryDefinition" >
      <ref bean="EarnCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="EarnCode-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="earnCodeId" />
    <property name="attributes" >
      <list>
        <ref bean="EarnCode-earnCodeId" />
        <ref bean="EarnCode-earnCode" />
        <ref bean="EarnCode-active" />
        <ref bean="EarnCode-description" />
        <ref bean="effectiveDate" />
       </list>
    </property>
    <property name="objectLabel" value="earnCode"/>
  </bean>


  <bean id="EarnCode-earnCodeId" parent="EarnCode-earnCodeId-parentBean"/>
  <bean id="EarnCode-earnCodeId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Earn Code Id"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="name" value="earnCodeId"/>
    <property name="label" value="Earn Code Id"/>
  </bean>
  

  
  
  <bean id="EarnCode-earnCode" parent="EarnCode-earnCode-parentBean" />
  <bean id="EarnCode-earnCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="maxLength" value="10" />
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>

    <property name="name" value="earnCode" />
    <property name="label" value="Earn Code" />
  </bean> 
  

  <bean id="EarnCode-active" parent="EarnCode-active-parentBean"/>
  <bean id="EarnCode-active-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Active"/>
    <property name="control">
      <bean parent="CheckboxControlDefinition"/>
    </property>
    <property name="name" value="active"/>
    <property name="label" value="Active"/>
  </bean>

 <bean id="EarnCode-description" parent="EarnCode-description-parentBean"/>
  <bean id="EarnCode-description-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Description"/>
    <property name="maxLength" value="30"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="name" value="description"/>
    <property name="label" value="Description"/>
  </bean>
  
   <bean id="EarnCode-effectiveDate" parent="EarnCode-effectiveDate-parentBean"/>
  <bean id="EarnCode-effectiveDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Effective Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlDateFormatter"/>
    <property name="name" value="effectiveDate"/>
    <property name="label" value="Effective Date"/>
  </bean>
  
  
 
<!-- Business Object Inquiry Definition -->


  <bean id="EarnCode-inquiryDefinition" parent="EarnCode-inquiryDefinition-parentBean" />

  <bean id="EarnCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="earnCodeId" />
              <bean parent="FieldDefinition" p:attributeName="earnCode" />
            </list>
          </property>
          <property name="title" value="EarnCode" />
        </bean>
      </list>
    </property>
    <property name="title" value="EarnCode Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="EarnCode-lookupDefinition" parent="EarnCode-lookupDefinition-parentBean" />

  <bean id="EarnCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition" p:attributeName="earnCodeId" />
        <bean parent="FieldDefinition" p:attributeName="earnCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition" p:attributeName="earnCodeId" p:forceInquiry="true" />
        <bean parent="FieldDefinition" p:attributeName="earnCode" />
        <bean parent="FieldDefinition" p:attributeName="active" />
      </list>
    </property>
    <property name="title" value="EarnCode Lookup" />
  </bean>
</beans>
