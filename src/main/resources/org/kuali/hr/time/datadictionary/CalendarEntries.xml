<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">


  <bean id="CalendarEntries" parent="CalendarEntries-parentBean"/>
  <bean id="CalendarEntries-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.hr.time.calendar.CalendarEntries"/>
    <property name="titleAttribute" value="hrCalendarEntriesId"/>
    <property name="objectLabel" value="CalendarEntries"/>
    <property name="lookupDefinition">
      <ref bean="CalendarEntries-lookupDefinition"/>
    </property>
     <property name="inquiryDefinition">
		<ref bean="CalendarEntries-inquiryDefinition" />
	</property>
    <property name="attributes">
      <list>
        <ref bean="CalendarEntries-hrCalendarId"/>
        <ref bean="CalendarEntries-calendarName"/>
        <ref bean="CalendarEntries-beginPeriodDateTime"/>
		<ref bean="CalendarEntries-beginPeriodDate"/>
		<ref bean="CalendarEntries-beginPeriodTime"/>
		
        <ref bean="CalendarEntries-endPeriodDateTime"/>
		<ref bean="CalendarEntries-endPeriodDate"/>
		<ref bean="CalendarEntries-endPeriodTime"/>        
        
        <ref bean="CalendarEntries-initiateDate"/>
        <ref bean="CalendarEntries-initiateTime"/>
        <ref bean="CalendarEntries-endPayPeriodDate"/>
        <ref bean="CalendarEntries-endPayPeriodTime"/>
        <ref bean="CalendarEntries-employeeApprovalDate"/>
        <ref bean="CalendarEntries-employeeApprovalTime"/>
        <ref bean="CalendarEntries-supervisorApprovalDate"/>
        <ref bean="CalendarEntries-supervisorApprovalTime"/>        
      </list>
    </property>   
    
    <property name="relationships">
  		<list>
             <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="calendarObj"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="hrCalendarId" p:targetName="hrCalendarId"/>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="calendarName" p:targetName="calendarName"/>
                </list>
              </property>
            </bean>
    	</list>
    </property>
  </bean>

  <!-- Attribute Definitions -->
  
  <bean id="CalendarEntries-hrCalendarId" parent="CalendarEntries-hrCalendarId-parentBean"/>
  <bean id="CalendarEntries-hrCalendarId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Id"/>
    <property name="maxLength" value="20"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="20"/>
    </property>
    <property name="name" value="hrCalendarId"/>
    <property name="label" value="Calendar Id"/>
  </bean>
  
	<bean id="CalendarEntries-calendarName" parent="CalendarEntries-calendarName-parentBean" />
	<bean id="CalendarEntries-calendarName-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="shortLabel" value="Calendar Name"/>
		<property name="forceUppercase" value="true" />
		<property name="maxLength" value="30" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="30" />
		</property>
		<property name="name" value="calendarName" />
		<property name="label" value="Calendar Name" />
		<property name="description" value="calendarName" />
	</bean>
  
    
  <bean id="CalendarEntries-beginPeriodDateTime" parent="CalendarEntries-beginPeriodDateTime-parentBean"/>
  <bean id="CalendarEntries-beginPeriodDateTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Begin Pay Period Date Time"/>  <!-- need to use different labels for lookup and search results fields.Had to make short label longer than label, useShortLabel not working for lookup fields -->
    <property name="maxLength" value="30"/>
     <property name="validationPattern">
 		<bean parent="TimestampValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="30" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.core.web.format.TimestampAMPMFormatter"/>
    <property name="name" value="beginPeriodDateTime"/>
    <property name="label" value="Begin Pay Period Date"/>
  </bean>
  
    
  <bean id="CalendarEntries-beginPeriodDate" parent="CalendarEntries-beginPeriodDate-parentBean"/>
  <bean id="CalendarEntries-beginPeriodDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Begin Pay Period Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.core.web.format.DateFormatter"/>
    <property name="name" value="beginPeriodDate"/>
    <property name="label" value="Begin Pay Period Date"/>
  </bean>  

  <bean id="CalendarEntries-beginPeriodTime" parent="CalendarEntries-beginPeriodTime-parentBean"/>
  <bean id="CalendarEntries-beginPeriodTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Begin Pay Period Time"/>
    <property name="maxLength" value="10"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>    
    <property name="validationPattern">
 		<bean parent="RegexValidationPattern" p:pattern="[0-9]{2}:[0-9]{2}\s*[Aa|Pp][Mm]" p:validationErrorMessageKey="MISSING"/>
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlTimeFormatter"/>
    <property name="name" value="beginPeriodTime"/>
    <property name="label" value="Begin Pay Period Time (00:00 AM)"/>
  </bean>

  <bean id="CalendarEntries-endPeriodDateTime" parent="CalendarEntries-endPeriodDateTime-parentBean"/>
  <bean id="CalendarEntries-endPeriodDateTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="End Pay Period Date Time"/> <!-- need to use different labels for lookup and search results fields.Had to make shortLabel longer than label, useShortLabel not working for lookup fields -->
    <property name="maxLength" value="30"/>
    <property name="validationPattern">
 		<bean parent="TimestampValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="30" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.core.web.format.TimestampAMPMFormatter"/>
    <property name="name" value="endPeriodDateTime"/>
    <property name="label" value="End Pay Period Date"/>
  </bean>
  
  <bean id="CalendarEntries-endPeriodDate" parent="CalendarEntries-endPeriodDate-parentBean"/>
  <bean id="CalendarEntries-endPeriodDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="End Pay Period Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.core.web.format.DateFormatter"/>
    <property name="name" value="endPeriodDate"/>
    <property name="label" value="End Pay Period Date"/>
  </bean>    
  
  <bean id="CalendarEntries-endPeriodTime" parent="CalendarEntries-endPeriodTime-parentBean"/>
  <bean id="CalendarEntries-endPeriodTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="End Pay Period Time"/>
    <property name="maxLength" value="10"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>    
    <property name="validationPattern">
 		<bean parent="RegexValidationPattern" p:pattern="[0-9]{2}:[0-9]{2}\s*[Aa|Pp][Mm]" p:validationErrorMessageKey="MISSING"/>
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlTimeFormatter"/>
    <property name="name" value="endPeriodTime"/>
    <property name="label" value="End Pay Period Time (00:00 AM)"/>
  </bean>
    
  <bean id="CalendarEntries-initiateDate" parent="CalendarEntries-initiateDate-parentBean"/>
  <bean id="CalendarEntries-initiateDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch Initiate Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.core.web.format.DateFormatter"/>
    <property name="name" value="batchInitiateDate"/>
    <property name="label" value="Batch Initiate Date"/>
  </bean>

  <bean id="CalendarEntries-initiateTime" parent="CalendarEntries-initiateTime-parentBean"/>
  <bean id="CalendarEntries-initiateTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch Initiate Time"/>
    <property name="maxLength" value="10"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>    
    <property name="validationPattern">
 		<bean parent="RegexValidationPattern" p:pattern="[0-9]{2}:[0-9]{2}\s*[Aa|Pp][Mm]" p:validationErrorMessageKey="MISSING"/>
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlTimeFormatter"/>
    <property name="name" value="batchInitiateTime"/>
    <property name="label" value="Batch Initiate Time (00:00 AM)"/>
  </bean>

  <!--  -->

  <bean id="CalendarEntries-endPayPeriodDate" parent="CalendarEntries-endPayPeriodDate-parentBean"/>
  <bean id="CalendarEntries-endPayPeriodDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch End Pay Period Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.core.web.format.DateFormatter"/>
    <property name="name" value="batchEndPayPeriodDate"/>
    <property name="label" value="Batch End Pay Period Date"/>
  </bean>

  <bean id="CalendarEntries-endPayPeriodTime" parent="CalendarEntries-endPayPeriodTime-parentBean"/>
  <bean id="CalendarEntries-endPayPeriodTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch End Pay Period Time"/>
    <property name="maxLength" value="10"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>    
    <property name="validationPattern">
 		<bean parent="RegexValidationPattern" p:pattern="[0-9]{2}:[0-9]{2}\s*[Aa|Pp][Mm]" p:validationErrorMessageKey="MISSING"/>
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlTimeFormatter"/>
    <property name="name" value="batchEndPayPeriodTime"/>
    <property name="label" value="Batch End Pay Period Time (00:00 AM)"/>
  </bean>

  <!--  -->

  <bean id="CalendarEntries-employeeApprovalDate" parent="CalendarEntries-employeeApprovalDate-parentBean"/>
  <bean id="CalendarEntries-employeeApprovalDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch Employee Approval Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.core.web.format.DateFormatter"/>
    <property name="name" value="batchEmployeeApprovalDate"/>
    <property name="label" value="Batch Employee Approval Date"/>
  </bean>

  <bean id="CalendarEntries-employeeApprovalTime" parent="CalendarEntries-employeeApprovalTime-parentBean"/>
  <bean id="CalendarEntries-employeeApprovalTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch Employee Approval Time"/>
    <property name="maxLength" value="10"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>    
    <property name="validationPattern">
 		<bean parent="RegexValidationPattern" p:pattern="[0-9]{2}:[0-9]{2}\s*[Aa|Pp][Mm]" p:validationErrorMessageKey="MISSING"/>
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlTimeFormatter"/>
    <property name="name" value="batchEmployeeApprovalTime"/>
    <property name="label" value="Batch Employee Approval Time (00:00 AM)"/>
  </bean>

  <!--  -->

  <bean id="CalendarEntries-supervisorApprovalDate" parent="CalendarEntries-supervisorApprovalDate-parentBean"/>
  <bean id="CalendarEntries-supervisorApprovalDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch Supervisor Approval Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.core.web.format.DateFormatter"/>
    <property name="name" value="batchSupervisorApprovalDate"/>
    <property name="label" value="Batch Supervisor Approval Date"/>
  </bean>

  <bean id="CalendarEntries-supervisorApprovalTime" parent="CalendarEntries-supervisorApprovalTime-parentBean"/>
  <bean id="CalendarEntries-supervisorApprovalTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch Supervisor Approval Time"/>
    <property name="maxLength" value="10"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>    
    <property name="validationPattern">
 		<bean parent="RegexValidationPattern" p:pattern="[0-9]{2}:[0-9]{2}\s*[Aa|Pp][Mm]" p:validationErrorMessageKey="MISSING"/>
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlTimeFormatter"/>
    <property name="name" value="batchSupervisorApprovalTime"/>
    <property name="label" value="Batch Supervisor Approval Time (00:00 AM)"/>
  </bean>
  


  <bean id="CalendarEntries-lookupDefinition" parent="CalendarEntries-lookupDefinition-parentBean"/>
  <bean id="CalendarEntries-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="lookupableID" value="calendarEntriesLookupable"></property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="calendarName"/>
        <bean parent="FieldDefinition" p:attributeName="hrCalendarId" p:noInquiry="true" p:noLookup="true" p:hidden="true"/>
        <bean parent="FieldDefinition" p:attributeName="beginPeriodDateTime"/>
        <bean parent="FieldDefinition" p:attributeName="endPeriodDateTime"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="calendarName"/>
        <bean parent="FieldDefinition" p:attributeName="beginPeriodDateTime" p:useShortLabel="true"/>
        <bean parent="FieldDefinition" p:attributeName="endPeriodDateTime"  p:useShortLabel="true"/>
      </list>
    </property>
    <property name="title" value="Calendar Entry Lookup"/>
  </bean>
  
   <!-- Inquiry Definition -->
  
	<bean id="CalendarEntries-inquiryDefinition" parent="CalendarEntries-inquiryDefinition-parentBean" />
	<bean id="CalendarEntries-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" p:attributeName="calendarName"/>
					        <bean parent="FieldDefinition" p:attributeName="beginPeriodDateTime" p:useShortLabel="true"/>
					        <bean parent="FieldDefinition" p:attributeName="endPeriodDateTime" p:useShortLabel="true"/>
					        <bean parent="FieldDefinition" p:attributeName="batchInitiateDate" />
					        <bean parent="FieldDefinition" p:attributeName="batchInitiateTime" />
					        <bean parent="FieldDefinition" p:attributeName="batchEndPayPeriodDate" />
					        <bean parent="FieldDefinition" p:attributeName="batchEndPayPeriodTime" />
					        <bean parent="FieldDefinition" p:attributeName="batchEmployeeApprovalDate" />
					        <bean parent="FieldDefinition" p:attributeName="batchEmployeeApprovalTime" />
					        <bean parent="FieldDefinition" p:attributeName="batchSupervisorApprovalDate" />
					        <bean parent="FieldDefinition" p:attributeName="batchSupervisorApprovalTime" />  
						</list>
					</property>
					<property name="title" value="Calendar Entry" />
				</bean>
			</list>
		</property>
		<property name="translateCodes" value="false"/>
		<property name="title" value="Calendar Entry Inquiry" />
	</bean>
  
  
   
</beans>
