  <bean id="CalendarEntries-batchEmployeeApprovalDate" parent="CalendarEntries-batchEmployeeApprovalDate-parentBean"/>
  <bean id="CalendarEntries-batchEmployeeApprovalDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch Employee Approval Date"/>
    <property name="maxLength" value="10"/>
