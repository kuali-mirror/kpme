    <property name="label" value="Batch Initiate Time (00:00 AM)"/>
  </bean>

  <!--  -->

  <bean id="CalendarEntries-batchEndPayPeriodDate" parent="CalendarEntries-batchEndPayPeriodDate-parentBean"/>
  <bean id="CalendarEntries-batchEndPayPeriodDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch End Pay Period Date"/>
    <property name="maxLength" value="10"/>
