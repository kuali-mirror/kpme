  <bean id="CalendarEntries-lookupDefinition" parent="CalendarEntries-lookupDefinition-parentBean"/>
  <bean id="CalendarEntries-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="lookupableID" value="calendarEntriesLookupable"></property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="calendarName"/>
        <bean parent="FieldDefinition" p:attributeName="hrCalendarId" p:noInquiry="true" p:noLookup="true" p:hidden="true"/>
        <bean parent="FieldDefinition" p:attributeName="beginPeriodDateTime"/>
        <bean parent="FieldDefinition" p:attributeName="endPeriodDateTime"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="calendarName"/>
        <bean parent="FieldDefinition" p:attributeName="beginPeriodDateTime" p:useShortLabel="true"/>
        <bean parent="FieldDefinition" p:attributeName="endPeriodDateTime"  p:useShortLabel="true"/>
      </list>
    </property>
    <property name="title" value="Calendar Entry Lookup"/>
  </bean>
  
   <!-- Inquiry Definition -->
  
	<bean id="CalendarEntries-inquiryDefinition" parent="CalendarEntries-inquiryDefinition-parentBean" />
	<bean id="CalendarEntries-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" p:attributeName="calendarName"/>
					        <bean parent="FieldDefinition" p:attributeName="beginPeriodDateTime" p:useShortLabel="true"/>
					        <bean parent="FieldDefinition" p:attributeName="endPeriodDateTime" p:useShortLabel="true"/>
					        <bean parent="FieldDefinition" p:attributeName="batchInitiateDate" />
					        <bean parent="FieldDefinition" p:attributeName="batchInitiateTime" />
					        <bean parent="FieldDefinition" p:attributeName="batchEndPayPeriodDate" />
					        <bean parent="FieldDefinition" p:attributeName="batchEndPayPeriodTime" />
					        <bean parent="FieldDefinition" p:attributeName="batchEmployeeApprovalDate" />
					        <bean parent="FieldDefinition" p:attributeName="batchEmployeeApprovalTime" />
					        <bean parent="FieldDefinition" p:attributeName="batchSupervisorApprovalDate" />
					        <bean parent="FieldDefinition" p:attributeName="batchSupervisorApprovalTime" />  
						</list>
					</property>
					<property name="title" value="Calendar Entry" />
				</bean>
			</list>
		</property>
		<property name="translateCodes" value="false"/>
		<property name="title" value="Calendar Entry Inquiry" />
	</bean>
  
  
   
</beans>
