 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.core.web.format.DateFormatter"/>
    <property name="name" value="batchEmployeeApprovalDate"/>
    <property name="label" value="Batch Employee Approval Date"/>
  </bean>

  <bean id="CalendarEntries-batchEmployeeApprovalTime" parent="CalendarEntries-batchEmployeeApprovalTime-parentBean"/>
  <bean id="CalendarEntries-batchEmployeeApprovalTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch Employee Approval Time"/>
    <property name="maxLength" value="10"/>
    <property name="control">
