  </bean>

  <bean id="CalendarEntries-batchEndPayPeriodTime" parent="CalendarEntries-batchEndPayPeriodTime-parentBean"/>
  <bean id="CalendarEntries-batchEndPayPeriodTime-parentBean" abstract="true" parent="AttributeDefinition">
