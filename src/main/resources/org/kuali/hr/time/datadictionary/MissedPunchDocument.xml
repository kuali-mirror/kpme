<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:dd="http://rice.kuali.org/dd"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

	<bean id="MissedPunchDocument" parent="MissedPunchDocument-parentBean" />

	<bean id="MissedPunchDocument-parentBean" abstract="true"
		parent="TransactionalDocumentEntry">
		<property name="documentTypeName" value="MissedPunchDocumentType" />
		<property name="documentClass"
			value="org.kuali.hr.time.missedpunch.MissedPunchDocument" />
		<property name="businessRulesClass" value="org.kuali.hr.time.missedpunch.MissedPunchValidation"/>			
		<property name="attributes">
			<list>
				<ref bean="MissedPunchDocument-principalId" />
				<ref bean="MissedPunchDocument-assignment" />
				<ref bean="MissedPunchDocument-clockAction" />
				<ref bean="MissedPunchDocument-actionDate" />
				<ref bean="MissedPunchDocument-actionTime" />
				<ref bean="MissedPunchDocument-documentId" />
				<ref bean="MissedPunchDocument-timesheetDocumentId" />
				<ref bean="MissedPunchDocument-documentStatus" />
			</list>
		</property>
	</bean>
	
	<bean id="MissedPunchDocument-assignment" parent="MissedPunchDocument-assignment-parentBean" />
	<bean id="MissedPunchDocument-assignment-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false"/>
		<property name="shortLabel" value="Assignment" />
		<property name="maxLength" value="30" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
		  <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.hr.time.missedpunch.MissedPunchAssignmentFinder" p:includeKeyInLabel="false"/>
		</property>
		<property name="name" value="assignment" />
		<property name="label" value="Assignment" />
	</bean>


    <bean id="MissedPunchDocument-principalId" parent="MissedPunchDocument-principalId-parentBean" />
    <bean id="MissedPunchDocument-principalId-parentBean" abstract="true"
        parent="AttributeDefinition">
        <property name="shortLabel" value="Principal Id" />
        <property name="maxLength" value="20" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
                <property name="allowWhitespace" value="false" />
            </bean>
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="40" />
        </property>
        <property name="name" value="principalId" />
        <property name="label" value="Principal Id" />
    </bean>

    <bean id="MissedPunchDocument-clockAction" parent="MissedPunchDocument-clockAction-parentBean" />
    <bean id="MissedPunchDocument-clockAction-parentBean" abstract="true"
        parent="AttributeDefinition">
        <property name="shortLabel" value="Clock Action" />
        <property name="maxLength" value="30" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
                <property name="allowWhitespace" value="false" />
            </bean>
        </property>
        <property name="control">
            <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.hr.time.clocklog.TkClockActionValuesFinder" p:includeKeyInLabel="false"/>
        </property>
        <property name="name" value="clockAction" />
        <property name="label" value="Clock Action" />
    </bean>

    <bean id="MissedPunchDocument-actionDate" parent="MissedPunchDocument-actionDate-parentBean"/>
    <bean id="MissedPunchDocument-actionDate-parentBean" abstract="true" parent="AttributeDefinition">
      <property name="shortLabel" value="Action Date"/>
      <property name="maxLength" value="12"/>
      <property name="validationPattern">
          <bean parent="DateValidationPattern"/>
      </property>
      <property name="control">
        <bean parent="TextControlDefinition" p:size="12" p:datePicker="true" />
      </property>
      <property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter"/>
      <property name="name" value="actionDate"/>
      <property name="label" value="Action Date"/>
    </bean>

    <bean id="MissedPunchDocument-actionTime" parent="MissedPunchDocument-actionTime-parentBean"/>
    <bean id="MissedPunchDocument-actionTime-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Action Time"/>
        <property name="maxLength" value="10"/>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="10"/>
        </property>
        <property name="validationPattern">
            <bean parent="RegexValidationPattern" p:pattern="[0-9]{2}:[0-9]{2}\s*[Aa|Pp][Mm]" p:validationErrorMessageKey="MISSING"/>
        </property>
        <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlTimeFormatter"/>
        <property name="name" value="actionTime"/>
        <property name="label" value="Action Time"/>
    </bean>

    <bean id="MissedPunchDocument-documentId" parent="MissedPunchDocument-documentId-parentBean"/>
    <bean id="MissedPunchDocument-documentId-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Document Id"/>
        <property name="maxLength" value="14"/>
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
            <property name="allowWhitespace" value="false" />
            </bean>
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="14"/>
        </property>
        <property name="name" value="documentId"/>
        <property name="label" value="Document Id"/>
    </bean>

    <bean id="MissedPunchDocument-timesheetDocumentId" parent="MissedPunchDocument-timesheetDocumentId-parentBean"/>
    <bean id="MissedPunchDocument-timesheetDocumentId-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Timesheet Doc Id"/>
        <property name="maxLength" value="14"/>
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
            <property name="allowWhitespace" value="false" />
            </bean>
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="14"/>
        </property>
        <property name="name" value="timesheetDocumentId"/>
        <property name="label" value="Timesheet Document Id"/>
    </bean>

    <bean id="MissedPunchDocument-documentStatus" parent="MissedPunchDocument-documentStatus-parentBean" />
    <bean id="MissedPunchDocument-documentStatus-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="maxLength" value="10" />
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="10" />
        </property>
        <property name="name" value="documentStatus" />
        <property name="label" value="Document Status" />
    </bean>
    <!--  Inquiry Definitions  -->


	

</beans>