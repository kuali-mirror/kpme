<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="WorkSchedule" parent="WorkSchedule-parentBean" />

	<bean id="WorkSchedule-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.hr.time.workschedule.WorkSchedule" />
		<property name="titleAttribute" value="hrWorkScheduleId" />
		<property name="objectLabel" value="WorkSchedule" />
		<property name="lookupDefinition">
			<ref bean="WorkSchedule-lookupDefinition" />
		</property>
		<property name="attributes">
			<list>
				<ref bean="WorkSchedule-WorkScheduleDesc" />
				<ref bean="WorkSchedule-effectiveDate" />
				<ref bean="WorkSchedule-dept" />
				<ref bean="WorkSchedule-workArea" />
				<ref bean="WorkSchedule-principalId" />
				<ref bean="WorkSchedule-active" />
			</list>
		</property>
		 <property name="relationships">
  		<list>
            <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="workAreaObj"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="workArea" p:targetName="workAreaId"/>
                </list>
              </property>
            </bean>

            <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="department"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="deptId" p:targetName="deptId"/>
                </list>
              </property>
            </bean>             

        </list>
    </property>
		<property name="collections">
			<list>
				<bean parent="CollectionDefinition" p:name="workScheduleEntries"
					p:label="WorkScheduleEntries" p:shortLabel="WorkScheduleEntries"
					p:summary="WorkScheduleEntries" p:description="WorkScheduleEntries" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	<bean id="WorkSchedule-WorkScheduleDesc" parent="WorkSchedule-WorkScheduleDesc-parentBean" />
	<bean id="WorkSchedule-WorkScheduleDesc-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Description" />
		<property name="maxLength" value="30" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="30" />
		</property>
		<property name="name" value="workScheduleDesc" />
		<property name="label" value="Description" />
	</bean>

	<bean id="WorkSchedule-effectiveDate" parent="WorkSchedule-effectiveDate-parentBean" />
	<bean id="WorkSchedule-effectiveDate-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Effective Date" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="DateValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
		</property>
		<property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter" />
		<property name="name" value="effectiveDate" />
		<property name="label" value="Effective Date" />
	</bean>

	<bean id="WorkSchedule-dept" parent="WorkSchedule-dept-parentBean" />
	<bean id="WorkSchedule-dept-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Department Id" />
		<property name="maxLength" value="21" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="21" />
		</property>
		<property name="name" value="dept" />
		<property name="label" value="Department Id" />
	</bean>

	<bean id="WorkSchedule-workArea" parent="WorkSchedule-workArea-parentBean" />
	<bean id="WorkSchedule-workArea-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Work Area" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="workArea" />
		<property name="label" value="Work Area" />
	</bean>

	<bean id="WorkSchedule-principalId" parent="WorkSchedule-principalId-parentBean" />
	<bean id="WorkSchedule-principalId-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Principal Id" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="principalId" />
		<property name="label" value="Principal Id" />
	</bean>

	<bean id="WorkSchedule-active" parent="WorkSchedule-active-parentBean" />
	<bean id="WorkSchedule-active-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Active" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="name" value="active" />
		<property name="label" value="Active" />
	</bean>

	<!--  Inquiry Definitions  -->
	<bean id="WorkSchedule-lookupDefinition" parent="WorkSchedule-lookupDefinition-parentBean" />
	<bean id="WorkSchedule-lookupDefinition-parentBean" abstract="true"
		parent="LookupDefinition">
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="principalId" />
				<bean parent="FieldDefinition" p:attributeName="workArea" />
				<bean parent="FieldDefinition" p:attributeName="dept" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="principalId" />
				<bean parent="FieldDefinition" p:attributeName="workScheduleDesc" />
				<bean parent="FieldDefinition" p:attributeName="effectiveDate" />
				<bean parent="FieldDefinition" p:attributeName="workArea" />
				<bean parent="FieldDefinition" p:attributeName="dept" />
			</list>
		</property>
		<property name="title" value="WorkSchedule Lookup" />
	</bean>
</beans>
