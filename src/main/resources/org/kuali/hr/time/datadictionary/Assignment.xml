<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Assignment" parent="Assignment-parentBean"/>

  <bean id="Assignment-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.hr.time.assignment.Assignment"/>
    <property name="titleAttribute" value="id"/>
    <property name="objectLabel" value="Assignment"/>
    <property name="lookupDefinition">
      <ref bean="Assignment-lookupDefinition"/>
    </property>
    <property name="attributes">
      <list>      	
        <ref bean="Assignment-principalId"/>
        <ref bean="Assignment-jobNumber"/>
        <ref bean="Assignment-effectiveDate"/>
        <ref bean="Assignment-earnCode"/>
        <ref bean="Assignment-workAreaId"/>
        <ref bean="Assignment-taskId"/>
        <ref bean="Assignment-active"/>
      </list>
    </property>
     <property name="relationships">
  		<list>
            <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="workArea"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="workAreaId" p:targetName="workAreaId"/>
                </list>
              </property>
            </bean>    

            <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="task"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="taskId" p:targetName="taskId"/>
                </list>
              </property>
            </bean>
            
            <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="job"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="principalId" p:targetName="principalId"/>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="jobNumber" p:targetName="jobNumber"/>
                </list>
              </property>
            </bean>            
                    
        </list>
    </property>       
  </bean>

  <!-- Attribute Definitions -->  
  <bean id="Assignment-principalId" parent="Assignment-principalId-parentBean"/>
  <bean id="Assignment-principalId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Principal Id"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern">
      	<property name="allowWhitespace" value="false"/>
      </bean>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="name" value="principalId"/>
    <property name="label" value="Principal Id"/>
  </bean>

   <bean id="Assignment-jobNumber" parent="Assignment-jobNumber-parentBean"/>
  <bean id="Assignment-jobNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Job Number"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="3"/>
    </property>
    <property name="name" value="jobNumber"/>
    <property name="label" value="Job Number"/>
  </bean>

  <bean id="Assignment-effectiveDate" parent="Assignment-effectiveDate-parentBean"/>
  <bean id="Assignment-effectiveDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Effective Date"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="12" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter"/>
    <property name="name" value="effectiveDate"/>
    <property name="label" value="Effective Date"/>
  </bean>

  <bean id="Assignment-earnCode" parent="Assignment-earnCode-parentBean"/>
  <bean id="Assignment-earnCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Earn Code"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern">
      	<property name="allowWhitespace" value="false"/>
      </bean>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="3"/>
    </property>
    <property name="name" value="earnCode"/>
    <property name="label" value="Earn Code"/>
  </bean>
  
  <bean id="Assignment-workAreaId" parent="Assignment-workAreaId-parentBean"/>
  <bean id="Assignment-workAreaId-parentBean" abstract="true" parent="AttributeDefinition">
     <property name="shortLabel" value="Work Area"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="name" value="workAreaId"/>
    <property name="label" value="Work Area"/>
  </bean>  
  
  <bean id="Assignment-taskId" parent="Assignment-taskId-parentBean"/>
  <bean id="Assignment-taskId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Task ID"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="name" value="taskId"/>
    <property name="label" value="Task ID"/>
  </bean>     
  
  <bean id="Assignment-active" parent="Assignment-active-parentBean"/>
  <bean id="Assignment-active-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Active"/>
    <property name="control">
      <bean parent="CheckboxControlDefinition"/>
    </property>
    <property name="name" value="active"/>
    <property name="label" value="Active"/>
  </bean>
  
  <!--  Inquiry Definitions  -->

	
  <bean id="Assignment-lookupDefinition" parent="Assignment-lookupDefinition-parentBean"/>
  <bean id="Assignment-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="lookupFields">
      <list>       
        <bean parent="FieldDefinition" p:attributeName="principalId"/>
		<bean parent="FieldDefinition" p:attributeName="workAreaId"/>
      </list>
    </property>
    <property name="resultFields">
      <list>       
        <bean parent="FieldDefinition" p:attributeName="principalId"/>
        <bean parent="FieldDefinition" p:attributeName="jobNumber"/>
        <bean parent="FieldDefinition" p:attributeName="effectiveDate"/>
        <bean parent="FieldDefinition" p:attributeName="workArea"/>
        <bean parent="FieldDefinition" p:attributeName="taskId"/>           
      </list>
    </property>
    <property name="title" value="Assignment Lookup"/>
  </bean>

</beans>
