<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	<bean id="PayGrade" parent="PayGrade-parentBean" />

	<bean id="PayGrade-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.hr.paygrade.PayGrade" />
		<property name="titleAttribute" value="id" />
		<property name="objectLabel" value="PayGrade" />
		<property name="inquiryDefinition">
			<ref bean="PayGrade-inquiryDefinition" />
		</property>
		<property name="lookupDefinition">
			<ref bean="PayGrade-lookupDefinition" />
		</property>
		<property name="attributes">
			<list>
				<ref bean="PayGrade-hrPayGradeId" />
				<ref bean="PayGrade-payGrade" />
				<ref bean="PayGrade-description" />
				<ref bean="PayGrade-effectiveDate" />
				<ref bean="PayGrade-active" />
				<ref bean="PayGrade-userPrincipalId" />
				<ref bean="PayGrade-timestamp" />
			</list>
		</property>	 
	</bean>

	<!-- Attribute Definitions -->

	<bean id="PayGrade-hrPayGradeId" parent="PayGrade-hrPayGradeId-parentBean" />
	<bean id="PayGrade-hrPayGradeId-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="PayGrade Id" />
		<property name="maxLength" value="7" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="7" />
		</property>
		<property name="name" value="hrPayGradeId" />
		<property name="label" value="PayGrade Id" />
	</bean>

	<bean id="PayGrade-payGrade" parent="PayGrade-payGrade-parentBean" />
	<bean id="PayGrade-payGrade-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="payGrade" />
		<property name="maxLength" value="20" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="20" />
		</property>
		<property name="name" value="payGrade" />
		<property name="label" value="PayGrade" />
	</bean>

	<bean id="PayGrade-description" parent="PayGrade-description-parentBean" />
	<bean id="PayGrade-description-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Description" />
		<property name="maxLength" value="40" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="true" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="40" />
		</property>
		<property name="name" value="description" />
		<property name="label" value="Description" />
	</bean>

	<bean id="PayGrade-effectiveDate" parent="PayGrade-effectiveDate-parentBean" />
	<bean id="PayGrade-effectiveDate-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Effective Date" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="DateValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
		</property>
		<property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter" />
		<property name="name" value="effectiveDate" />
		<property name="label" value="Effective Date" />
	</bean>

	<bean id="PayGrade-active" parent="PayGrade-active-parentBean" />
	<bean id="PayGrade-active-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Active" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="name" value="active" />
		<property name="label" value="Active" />
		<property name="formatterClass" value="org.kuali.rice.kns.web.format.BooleanFormatter"/>
	</bean>


	<bean id="PayGrade-userPrincipalId" parent="PayGrade-userPrincipalId-parentBean" />
	<bean id="PayGrade-userPrincipalId-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="User Principal Id" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="LookupReadonlyControlDefinition" p:size="15" />
		</property>
		<property name="name" value="userPrincipalId" />
		<property name="label" value="User Principal Id" />
	</bean>


	<bean id="PayGrade-timestamp" parent="PayGrade-timestamp-parentBean" />
	<bean id="PayGrade-timestamp-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Timestamp" />
		<property name="maxLength" value="15" />

		<property name="control">
			<bean parent="LookupReadonlyControlDefinition" p:size="15" />
		</property>
		<property name="formatterClass"
			value="org.kuali.rice.kns.web.format.TimestampAMPMFormatter" />
		<property name="name" value="timestamp" />
		<property name="label" value="Time Stamp" />
	</bean>
		<!--  Inquiry Definitions  -->
  
  	<bean id="PayGrade-inquiryDefinition" parent="PayGrade-inquiryDefinition-parentBean" />

	<bean id="PayGrade-inquiryDefinition-parentBean" abstract="true"
		parent="InquiryDefinition">
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" p:attributeName="effectiveDate" />
							<bean parent="FieldDefinition" p:attributeName="payGrade" />
							<bean parent="FieldDefinition" p:attributeName="description" />
							<bean parent="FieldDefinition" p:attributeName="active" />
						</list>
					</property>
					<property name="title" value="PayGrade" />
				</bean>
			</list>
		</property>
		<property name="title" value="PayGrade Inquiry" />
	</bean>

	<!-- Look up Definitions  -->

	<bean id="PayGrade-lookupDefinition" parent="PayGrade-lookupDefinition-parentBean" />
	<bean id="PayGrade-lookupDefinition-parentBean" abstract="true"
		parent="LookupDefinition">
		<property name="lookupableID" value="payGradeLookupable"></property>
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="payGrade" />
				<bean parent="FieldDefinition" p:attributeName="description" />
				<bean parent="FieldDefinition" p:attributeName="active"/>
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="payGrade" />
				<bean parent="FieldDefinition" p:attributeName="description" />
				<bean parent="FieldDefinition" p:attributeName="effectiveDate" />
				<bean parent="FieldDefinition" p:attributeName="timestamp" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
		<property name="title" value="PayGrade Lookup" />
	</bean>
</beans>

