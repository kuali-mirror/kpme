<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="WorkScheduleEntry" parent="WorkScheduleEntry-parentBean"/>

  <bean id="WorkScheduleEntry-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.hr.time.workschedule.WorkScheduleEntry"/>
    <property name="titleAttribute" value="hrWorkScheduleEntryId"/>
    <property name="objectLabel" value="WorkScheduleEntry"/>
    <property name="lookupDefinition">
      <ref bean="WorkScheduleEntry-lookupDefinition"/>
    </property>
    <property name="attributes">
      <list>
        <ref bean="WorkScheduleEntry-hrWorkSchedule"/>
        <ref bean="WorkScheduleEntry-indexOfDay"/>
      </list>
    </property>
  </bean>

  <!-- Attribute Definitions -->

  <bean id="WorkScheduleEntry-hrWorkSchedule" parent="WorkScheduleEntry-hrWorkSchedule-parentBean"/>
  <bean id="WorkScheduleEntry-hrWorkSchedule-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="WorkSchedule Id"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="name" value="hrWorkSchedule"/>
    <property name="label" value="WorkSchedule"/>
  </bean>

  <bean id="WorkScheduleEntry-indexOfDay" parent="WorkScheduleEntry-indexOfDay-parentBean"/>
  <bean id="WorkScheduleEntry-indexOfDay-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Index of period"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="name" value="indexOfDay"/>
    <property name="label" value="Index Of Day"/>
  </bean>


  <!--  Inquiry Definitions  -->

  <bean id="WorkScheduleEntry-lookupDefinition" parent="WorkScheduleEntry-lookupDefinition-parentBean"/>
  <bean id="WorkScheduleEntry-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="hrWorkSchedule"/>
	  </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="hrWorkSchedule"/>
      </list>
    </property>
    <property name="title" value="WorkScheduleEntry Lookup"/>
  </bean>

</beans>
