<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="WorkScheduleEntry" parent="WorkScheduleEntry-parentBean"/>

  <bean id="WorkScheduleEntry-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.hr.time.workschedule.WorkScheduleEntry"/>
    <property name="titleAttribute" value="hrWorkScheduleEntryId"/>
    <property name="objectLabel" value="WorkScheduleEntry"/>   
    <property name="lookupDefinition">
      <ref bean="WorkScheduleEntry-lookupDefinition"/>
    </property>
    <property name="attributes">
      <list>      	
        <ref bean="WorkScheduleEntry-hrWorkScheduleId"/>
        <ref bean="WorkScheduleEntry-calendarGroup"/>
        <ref bean="WorkScheduleEntry-dayOfPeriod"/>        
        <ref bean="WorkScheduleEntry-regHours"/>         
      </list>
    </property>
    
     <property name="relationships">
  		<list>
            <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="workScheduleObj"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="hrWorkScheduleId" p:targetName="hrWorkScheduleId"/>
                </list>
              </property>
            </bean> 
            
            <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="payCalendarObj"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="calendarGroup" p:targetName="calendarGroup"/>
                </list>
              </property>
            </bean>   
                    
        </list>
    </property>  
  </bean>

  <!-- Attribute Definitions -->
    
  <bean id="WorkScheduleEntry-hrWorkScheduleId" parent="WorkScheduleEntry-hrWorkScheduleId-parentBean"/>
  <bean id="WorkScheduleEntry-hrWorkScheduleId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="WorkSchedule Id"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="name" value="hrWorkScheduleId"/>
    <property name="label" value="WorkSchedule Id"/>
  </bean>
  
  <bean id="WorkScheduleEntry-calendarGroup" parent="WorkScheduleEntry-calendarGroup-parentBean"/>
  <bean id="WorkScheduleEntry-calendarGroup-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Calendar Group"/>
    <property name="maxLength" value="19"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="19"/>
    </property>
    <property name="name" value="calendarGroup"/>
    <property name="label" value="Calendar Group"/>
  </bean>
  
  <bean id="WorkScheduleEntry-dayOfPeriod" parent="WorkScheduleEntry-dayOfPeriod-parentBean"/>
  <bean id="WorkScheduleEntry-dayOfPeriod-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Day of period"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="name" value="dayOfPeriod"/>
    <property name="label" value="Day of period"/>
  </bean>
  
  <bean id="WorkScheduleEntry-regHours" parent="WorkScheduleEntry-regHours-parentBean"/>
  <bean id="WorkScheduleEntry-regHours-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Regular hour"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="name" value="regHours"/>
    <property name="label" value="Regular hours"/>
  </bean>
  
  <!--  Inquiry Definitions  -->
  
  <bean id="WorkScheduleEntry-lookupDefinition" parent="WorkScheduleEntry-lookupDefinition-parentBean"/>
  <bean id="WorkScheduleEntry-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="hrWorkScheduleId"/>
	  </list>
    </property>
    <property name="resultFields">
      <list>       
        <bean parent="FieldDefinition" p:attributeName="hrWorkScheduleId"/>
        <bean parent="FieldDefinition" p:attributeName="calendarGroup"/>
        <bean parent="FieldDefinition" p:attributeName="dayOfPeriod"/>
        <bean parent="FieldDefinition" p:attributeName="regHours"/>                   
      </list>
    </property>
    <property name="title" value="WorkScheduleEntry Lookup"/>
  </bean>

</beans>
