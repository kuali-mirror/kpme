  <bean id="PayCalendarEntries-employeeApprovalDate" parent="PayCalendarEntries-employeeApprovalDate-parentBean"/>
  <bean id="PayCalendarEntries-employeeApprovalDate-parentBean" abstract="true" parent="AttributeDefinition">
