    </property>
    <property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter"/>
    <property name="name" value="batchInitiateDate"/>
    <property name="label" value="Batch Initiate Date"/>
  </bean>

  <bean id="PayCalendarEntries-initiateTime" parent="PayCalendarEntries-initiateTime-parentBean"/>
  <bean id="PayCalendarEntries-initiateTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch Initiate Time"/>
