  </bean>

  <bean id="PayCalendarEntries-employeeApprovalTime" parent="PayCalendarEntries-employeeApprovalTime-parentBean"/>
  <bean id="PayCalendarEntries-employeeApprovalTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch Employee Approval Time"/>
    <property name="maxLength" value="10"/>
    <property name="control">
