<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">


  <bean id="PayCalendarEntries" parent="PayCalendarEntries-parentBean"/>
  <bean id="PayCalendarEntries-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.hr.time.paycalendar.PayCalendarEntries"/>
    <property name="titleAttribute" value="payCalendarDatesId"/>
    <property name="objectLabel" value="PayCalendarEntries"/>
    <property name="lookupDefinition">
      <ref bean="PayCalendarEntries-lookupDefinition"/>
    </property>
     <property name="inquiryDefinition">
		<ref bean="PayCalendarEntries-inquiryDefinition" />
	</property>
    <property name="attributes">
      <list>
        <ref bean="PayCalendarEntries-payCalendarId"/>
        <ref bean="PayCalendarEntries-calendarGroup"/>
        <ref bean="PayCalendarEntries-beginPeriodDateTime"/>
		<ref bean="PayCalendarEntries-beginPeriodDate"/>
		<ref bean="PayCalendarEntries-beginPeriodTime"/>
		
        <ref bean="PayCalendarEntries-endPeriodDateTime"/>
		<ref bean="PayCalendarEntries-endPeriodDate"/>
		<ref bean="PayCalendarEntries-endPeriodTime"/>        
        
        <ref bean="PayCalendarEntries-initiateDate"/>
        <ref bean="PayCalendarEntries-initiateTime"/>
        <ref bean="PayCalendarEntries-endPayPeriodDate"/>
        <ref bean="PayCalendarEntries-endPayPeriodTime"/>
        <ref bean="PayCalendarEntries-employeeApprovalDate"/>
        <ref bean="PayCalendarEntries-employeeApprovalTime"/>
        <ref bean="PayCalendarEntries-supervisorApprovalDate"/>
        <ref bean="PayCalendarEntries-supervisorApprovalTime"/>        
      </list>
    </property>   
    
    <property name="relationships">
  		<list>
             <bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="payCalendarObj"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="calendarGroup" p:targetName="calendarGroup"/>
                </list>
              </property>
            </bean>
    	</list>
    </property>
  </bean>

  <!-- Attribute Definitions -->
  
  <bean id="PayCalendarEntries-payCalendarId" parent="PayCalendarEntries-payCalendarId-parentBean"/>
  <bean id="PayCalendarEntries-payCalendarId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Id"/>
    <property name="maxLength" value="20"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="20"/>
    </property>
    <property name="name" value="payCalendarId"/>
    <property name="label" value="Pay Calendar Id"/>
  </bean>
  
	<bean id="PayCalendarEntries-calendarGroup" parent="PayCalendarEntries-calendarGroup-parentBean" />
	<bean id="PayCalendarEntries-calendarGroup-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="shortLabel" value="Pay Calendar"/>
		<property name="forceUppercase" value="true" />
		<property name="maxLength" value="30" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="30" />
		</property>
		<property name="name" value="calendarGroup" />
		<property name="label" value="Pay Calendar" />
		<property name="description" value="calendarGroup" />
	</bean>
  
    
  <bean id="PayCalendarEntries-beginPeriodDateTime" parent="PayCalendarEntries-beginPeriodDateTime-parentBean"/>
  <bean id="PayCalendarEntries-beginPeriodDateTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Begin Pay Period Date Time"/>
    <property name="maxLength" value="30"/>
     <property name="validationPattern">
 		<bean parent="TimestampValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="30" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.kns.web.format.TimestampAMPMFormatter"/>
    <property name="name" value="beginPeriodDateTime"/>
    <property name="label" value="Begin Pay Period Date Time"/>
  </bean>
  
    
  <bean id="PayCalendarEntries-beginPeriodDate" parent="PayCalendarEntries-beginPeriodDate-parentBean"/>
  <bean id="PayCalendarEntries-beginPeriodDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Begin Pay Period Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter"/>
    <property name="name" value="beginPeriodDate"/>
    <property name="label" value="Begin Pay Period Date"/>
  </bean>  

  <bean id="PayCalendarEntries-beginPeriodTime" parent="PayCalendarEntries-beginPeriodTime-parentBean"/>
  <bean id="PayCalendarEntries-beginPeriodTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Begin Pay Period Time"/>
    <property name="maxLength" value="10"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>    
    <property name="validationPattern">
 		<bean parent="RegexValidationPattern" p:pattern="[0-9]{2}:[0-9]{2}\s*[Aa|Pp][Mm]" p:validationErrorMessageKey="MISSING"/>
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlTimeFormatter"/>
    <property name="name" value="beginPeriodTime"/>
    <property name="label" value="Begin Pay Period Time"/>
  </bean>

  <bean id="PayCalendarEntries-endPeriodDateTime" parent="PayCalendarEntries-endPeriodDateTime-parentBean"/>
