    <property name="shortLabel" value="End Pay Period Date Time"/>
    <property name="maxLength" value="30"/>
    <property name="validationPattern">
 		<bean parent="TimestampValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="30" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.kns.web.format.TimestampAMPMFormatter"/>
    <property name="name" value="endPeriodDateTime"/>
    <property name="label" value="End Pay Period Date Time"/>
  </bean>
  
  <bean id="PayCalendarEntries-endPeriodDate" parent="PayCalendarEntries-endPeriodDate-parentBean"/>
  <bean id="PayCalendarEntries-endPeriodDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="End Pay Period Date"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
 		<bean parent="DateValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
    </property>
    <property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter"/>
    <property name="name" value="endPeriodDate"/>
    <property name="label" value="End Pay Period Date"/>
  </bean>    
  
  <bean id="PayCalendarEntries-endPeriodTime" parent="PayCalendarEntries-endPeriodTime-parentBean"/>
  <bean id="PayCalendarEntries-endPeriodTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="End Pay Period Time"/>
