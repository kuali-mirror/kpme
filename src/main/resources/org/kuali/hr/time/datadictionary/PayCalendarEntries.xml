    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>    
    <property name="validationPattern">
 		<bean parent="RegexValidationPattern" p:pattern="[0-9]{2}:[0-9]{2}\s*[Aa|Pp][Mm]" p:validationErrorMessageKey="MISSING"/>
    </property>
    <property name="formatterClass" value="org.kuali.hr.time.rice.kns.web.format.SqlTimeFormatter"/>
    <property name="name" value="endPeriodTime"/>
    <property name="label" value="End Pay Period Time"/>
  </bean>
    
  <bean id="PayCalendarEntries-initiateDate" parent="PayCalendarEntries-initiateDate-parentBean"/>
  <bean id="PayCalendarEntries-initiateDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch Initiate Date"/>
    <property name="maxLength" value="10"/>
