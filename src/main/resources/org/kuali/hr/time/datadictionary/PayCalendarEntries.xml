    <property name="label" value="Batch Initiate Time"/>
  </bean>

  <!--  -->

  <bean id="PayCalendarEntries-endPayPeriodDate" parent="PayCalendarEntries-endPayPeriodDate-parentBean"/>
  <bean id="PayCalendarEntries-endPayPeriodDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Batch End Pay Period Date"/>
