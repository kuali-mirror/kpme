<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:dd="http://rice.kuali.org/dd"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
                           http://rice.kuali.org/dd
                           http://rice.kuali.org/dd/dd.xsd">

  <bean id="PositionTypeMaintenanceDocument" parent="PositionTypeMaintenanceDocument-parentBean"/>
  <bean id="PositionTypeMaintenanceDocument-parentBean" abstract="true" parent="uifMaintenanceDocumentEntry">
    <property name="dataObjectClass" value="org.kuali.hr.pm.positiontype.PositionType"/>
    <property name="maintainableClass" value="org.kuali.hr.pm.positiontype.service.PositionTypeMaintainableImpl"/>  
    <property name="documentTypeName" value="PositionTypeMaintenanceDocumentType"/>
    <property name="documentAuthorizerClass" value="org.kuali.hr.time.authorization.DepartmentAdminAuthorizer"/>
    <property name="businessRulesClass" value="org.kuali.hr.pm.positiontype.validation.PositionTypeValidation"/>
  </bean>
</beans>
