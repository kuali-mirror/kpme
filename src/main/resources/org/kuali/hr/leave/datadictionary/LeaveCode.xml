<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean name="LeaveCode" parent="LeaveCode-parentBean" />

	<bean id="LeaveCode-parentBean" abstract="true"
		parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.hr.lm.leavecode.LeaveCode" />
		<property name="titleAttribute" value="id" />
		<property name="objectLabel" value="leavePlan" />
		<property name="inquiryDefinition">
			<ref bean="LeaveCode-inquiryDefinition" />
		</property>
		<property name="lookupDefinition">
			<ref bean="LeaveCode-lookupDefinition" />
		</property>
		<property name="attributes">
			<list>
				<ref bean="LeaveCode-leavePlan" />
				<ref bean="LeaveCode-eligibleForAccrual" />
				<ref bean="LeaveCode-accrualCategory" />
				<ref bean="LeaveCode-earnCode" />
				<ref bean="LeaveCode-leaveCode" />
				<ref bean="LeaveCode-displayName" />
				<ref bean="LeaveCode-unitOfTime" />
				<ref bean="LeaveCode-fractionalTimeAllowed" />
				<ref bean="LeaveCode-roundingOption" />
				<ref bean="LeaveCode-allowScheduledLeave" />
				<ref bean="LeaveCode-fmla" />
				<ref bean="LeaveCode-workmansComp" />
				<ref bean="LeaveCode-defaultAmountofTime" />
				<ref bean="LeaveCode-employee" />
				<ref bean="LeaveCode-approver" />
				<ref bean="LeaveCode-departmentAdmin" />
				<ref bean="LeaveCode-effectiveDate" />
				<ref bean="LeaveCode-timestamp" />
				<ref bean="LeaveCode-history" />
				<ref bean="LeaveCode-active" />
			</list>
		</property>
	</bean>

	<!-- leavePlan -->
	<bean id="LeaveCode-leavePlan" parent="LeaveCode-leavePlan-parentBean" />
	<bean id="LeaveCode-leavePlan-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Leave Plan" />
		<property name="maxLength" value="15" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="leavePlan" />
		<property name="label" value="Leave Plan" />
	</bean>
	
	<!-- accrualCategory -->
	<bean id="LeaveCode-accrualCategory" parent="LeaveCode-accrualCategory-parentBean" />
	<bean id="LeaveCode-accrualCategory-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Accrual Category" />
		<property name="maxLength" value="15" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="accrualCategory" />
		<property name="label" value="Accrual Category" />
	</bean>
	
	<!-- earnCode -->
	<bean id="LeaveCode-earnCode" parent="LeaveCode-earnCode-parentBean" />
	<bean id="LeaveCode-earnCode-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Earn Code" />
		<property name="maxLength" value="15" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="earnCode" />
		<property name="label" value="Earn Code" />
	</bean>
	
	<!-- leaveCode -->
	<bean id="LeaveCode-leaveCode" parent="LeaveCode-leaveCode-parentBean" />
	<bean id="LeaveCode-leaveCode-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Leave Code" />
		<property name="maxLength" value="15" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="leaveCode" />
		<property name="label" value="Leave Code" />
	</bean>
	
	<!-- Display Name -->
	<bean id="LeaveCode-displayName" parent="LeaveCode-displayName-parentBean" />
	<bean id="LeaveCode-displayName-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Display Name" />
		<property name="maxLength" value="20" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="true" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="displayName" />
		<property name="label" value="Display Name" />
	</bean>
	
	<!-- unitOfTime -->
	<bean id="LeaveCode-unitOfTime" parent="LeaveCode-unitOfTime-parentBean" />
	<bean id="LeaveCode-unitOfTime-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Unit Of Time" />
				<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.lm.util.UnitOfTimeKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="unitOfTime" />
		<property name="label" value="Unit Of Time" />
	</bean>
	
	<!-- Fractional time allowed -->
	<bean id="LeaveCode-fractionalTimeAllowed" parent="LeaveCode-fractionalTimeAllowed-parentBean" />
	<bean id="LeaveCode-fractionalTimeAllowed-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Fractional time allowed" />
				<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.lm.util.FractionOptionsKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="fractionalTimeAllowed" />
		<property name="label" value="Fractional time allowed" />
	</bean>
	
	<!-- Rounding Option -->
	<bean id="LeaveCode-roundingOption" parent="LeaveCode-roundingOption-parentBean" />
	<bean id="LeaveCode-roundingOption-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Rounding Option" />
				<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.lm.util.RoundingOptionsKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="roundingOption" />
		<property name="label" value="Rounding Option" />
	</bean>
	
	<!-- 	Allow Scheduled Leave -->
	<bean id="LeaveCode-allowScheduledLeave" parent="LeaveCode-allowScheduledLeave-parentBean" />
	<bean id="LeaveCode-allowScheduledLeave-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Allow Scheduled Leave" />
		<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.lm.util.SwitchKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="allowScheduledLeave" />
		<property name="label" value="Allow Scheduled Leave" />
	</bean>
	
	<!-- 	FMLA -->
	<bean id="LeaveCode-fmla" parent="LeaveCode-fmla-parentBean" />
	<bean id="LeaveCode-fmla-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="FMLA" />
		<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.lm.util.SwitchKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="fmla" />
		<property name="label" value="FMLA" />
	</bean>
	
	<!-- 	Workman’s Comp -->
	<bean id="LeaveCode-workmansComp" parent="LeaveCode-workmansComp-parentBean" />
	<bean id="LeaveCode-workmansComp-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Workman’s Comp" />
		<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.lm.util.SwitchKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="workmansComp" />
		<property name="label" value="Workman’s Comp" />
	</bean>
	
	<!-- Default Amount of Time -->
    <bean id="LeaveCode-defaultAmountofTime" parent="LeaveCode-defaultAmountofTime-parentBean"/>
  	<bean id="LeaveCode-defaultAmountofTime-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Default Amount of Time" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="RegexValidationPattern">
 		   		<property name="pattern" value="([1-9]|1[0-9]|2[0-4])()"></property>
 		   		<property name="validationErrorMessageKey" value="error.hours"></property>
 			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="defaultAmountofTime" />
		<property name="label" value="Default Amount of Time" />
	</bean>
  	
	<!-- Eligible for Accrual -->
	<bean id="LeaveCode-eligibleForAccrual" parent="LeaveCode-eligibleForAccrual-parentBean" />
	<bean id="LeaveCode-eligibleForAccrual-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Eligible for Accrual" />
		<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.time.util.TkHistoryKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="eligibleForAccrual" />
		<property name="label" value="Eligible for Accrual" />
	</bean>
	
	<!-- Employee -->
	<bean id="LeaveCode-employee" parent="LeaveCode-employee-parentBean" />
	<bean id="LeaveCode-employee-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Employee" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="name" value="employee" />
		<property name="label" value="Employee" />
		<property name="formatterClass" value="org.kuali.rice.kns.web.format.BooleanFormatter"/>
	</bean>
	
	<!-- Approver -->
	<bean id="LeaveCode-approver" parent="LeaveCode-approver-parentBean" />
	<bean id="LeaveCode-approver-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Approver" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="name" value="approver" />
		<property name="label" value="Approver" />
		<property name="formatterClass" value="org.kuali.rice.kns.web.format.BooleanFormatter"/>
	</bean>
	
	<!-- Department Admin -->
	<bean id="LeaveCode-departmentAdmin" parent="LeaveCode-departmentAdmin-parentBean" />
	<bean id="LeaveCode-departmentAdmin-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Department Admin" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="name" value="departmentAdmin" />
		<property name="label" value="Department Admin" />
		<property name="formatterClass" value="org.kuali.rice.kns.web.format.BooleanFormatter"/>
	</bean>

	<!-- effectiveDate -->
	<bean id="LeaveCode-effectiveDate" parent="LeaveCode-effectiveDate-parentBean" />
	<bean id="LeaveCode-effectiveDate-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Effective Date" />
		<property name="maxLength" value="12" />
		<property name="validationPattern">
			<bean parent="DateValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="12" p:datePicker="true" />
		</property>
		<property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter" />
		<property name="name" value="effectiveDate" />
		<property name="label" value="Effective Date" />
	</bean>
	
	<!-- timeStamp -->
	<bean id="LeaveCode-timestamp" parent="LeaveCode-timestamp-parentBean" />
	<bean id="LeaveCode-timestamp-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Timestamp" />
		<property name="maxLength" value="15" />
		<property name="control">
			<bean parent="LookupReadonlyControlDefinition" p:size="15" />
		</property>
		<property name="formatterClass"
			value="org.kuali.rice.kns.web.format.TimestampAMPMFormatter" />
		<property name="name" value="timestamp" />
		<property name="label" value="Time Stamp" />
	</bean>
	
	<!-- history -->
	<bean id="LeaveCode-history" parent="LeaveCode-history-parentBean" />
	<bean id="LeaveCode-history-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Show History" />
		<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.time.util.TkHistoryKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="history" />
		<property name="label" value="Show History" />
	</bean>
	
	<!-- active -->
	<bean id="LeaveCode-active" parent="LeaveCode-active-parentBean" />
	<bean id="LeaveCode-active-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Active" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="name" value="active" />
		<property name="label" value="Active" />
		<property name="formatterClass" value="org.kuali.rice.kns.web.format.BooleanFormatter"/>
	</bean>


	
	<!--  Inquiry Definitions  -->
  
  	<bean id="LeaveCode-inquiryDefinition" parent="LeaveCode-inquiryDefinition-parentBean" />

	<bean id="LeaveCode-inquiryDefinition-parentBean" abstract="true"
		parent="InquiryDefinition">
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" p:attributeName="effectiveDate" />
							<bean parent="FieldDefinition" p:attributeName="leavePlan" />
							<bean parent="FieldDefinition" p:attributeName="eligibleForAccrual" />
							<bean parent="FieldDefinition" p:attributeName="accrualCategory" />
							<bean parent="FieldDefinition" p:attributeName="earnCode" />
							<bean parent="FieldDefinition" p:attributeName="leaveCode" />
							<bean parent="FieldDefinition" p:attributeName="displayName" />
							<bean parent="FieldDefinition" p:attributeName="unitOfTime" />
							<bean parent="FieldDefinition" p:attributeName="fractionalTimeAllowed" />
							<bean parent="FieldDefinition" p:attributeName="roundingOption" />
							<bean parent="FieldDefinition" p:attributeName="allowScheduledLeave" />
							<bean parent="FieldDefinition" p:attributeName="fmla" />
							<bean parent="FieldDefinition" p:attributeName="workmansComp" />
							<bean parent="FieldDefinition" p:attributeName="defaultAmountofTime" />
							<bean parent="FieldDefinition" p:attributeName="employee" />
							<bean parent="FieldDefinition" p:attributeName="approver" />
							<bean parent="FieldDefinition" p:attributeName="departmentAdmin" />
							<bean parent="FieldDefinition" p:attributeName="active" />
						</list>
					</property>
					<property name="title" value="LeaveCode" />
				</bean>
			</list>
		</property>
		<property name="translateCodes" value="false"/>
		<property name="title" value="LeaveCode Inquiry" />
	</bean>

	<!--  Look up Definitions  -->

	<bean id="LeaveCode-lookupDefinition" parent="LeaveCode-lookupDefinition-parentBean" />
	<bean id="LeaveCode-lookupDefinition-parentBean" abstract="true"
		parent="LookupDefinition">
		<property name="lookupableID" value="leaveCodeLookupable"></property>
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="effectiveDate" />
				<bean parent="FieldDefinition" p:attributeName="leavePlan" />
				<bean parent="FieldDefinition" p:attributeName="eligibleForAccrual" />
				<bean parent="FieldDefinition" p:attributeName="accrualCategory" />
				<bean parent="FieldDefinition" p:attributeName="earnCode" />
				<bean parent="FieldDefinition" p:attributeName="leaveCode" />
				<bean parent="FieldDefinition" p:attributeName="active"/>
				<bean parent="FieldDefinition" p:attributeName="history" p:defaultValue="N"/>

			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="effectiveDate" />
				<bean parent="FieldDefinition" p:attributeName="leavePlan" />
				<bean parent="FieldDefinition" p:attributeName="eligibleForAccrual" />
				<bean parent="FieldDefinition" p:attributeName="accrualCategory" />
				<bean parent="FieldDefinition" p:attributeName="earnCode" />
				<bean parent="FieldDefinition" p:attributeName="leaveCode" />
				<bean parent="FieldDefinition" p:attributeName="active"/>
			</list>
		</property>
		<property name="title" value="Leave Code Lookup" />
	</bean>

</beans>