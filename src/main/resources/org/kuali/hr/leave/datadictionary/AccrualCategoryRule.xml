	<!--  Max Payout Leave Code -->
	<bean id="LeaveAccrualCategoryRule-maxPayoutLeaveCode" parent="LeaveAccrualCategoryRule-maxPayoutLeaveCode-parentBean" />
	<bean id="LeaveAccrualCategoryRule-maxPayoutLeaveCode-parentBean" abstract="true"
