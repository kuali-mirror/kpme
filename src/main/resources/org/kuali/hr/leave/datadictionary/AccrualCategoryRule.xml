<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="LeaveLeaveAccrualCategoryRule" parent="LeaveLeaveAccrualCategoryRule-parentBean" />

	<bean id="LeaveLeaveAccrualCategoryRule-parentBean" abstract="true"
		parent="BusinessObjectEntry">
		<property name="businessObjectClass"
			value="org.kuali.hr.lm.accrual.LeaveAccrualCategoryRule" />
		<property name="titleAttribute" value="id" />
		<property name="objectLabel" value="Assignment Account" />
		<property name="lookupDefinition">
			<ref bean="LeaveAccrualCategoryRule-lookupDefinition" />
		</property>
		<property name="attributes">
			<list>
				<ref bean="LeaveAccrualCategoryRule-serviceUnitOfTime" />
				<ref bean="LeaveAccrualCategoryRule-start" />
				<ref bean="LeaveAccrualCategoryRule-end" />
				<ref bean="LeaveAccrualCategoryRule-accrualRate" />
				<ref bean="LeaveAccrualCategoryRule-maxBalance" />
				<ref bean="LeaveAccrualCategoryRule-maxBalanceActionFrequency" />
				<ref bean="LeaveAccrualCategoryRule-actionAtMaxBalance" />
				<ref bean="LeaveAccrualCategoryRule-maxBalanceTransferToAccrualCategory" />
				<ref bean="LeaveAccrualCategoryRule-maxBalanceTransferConversionFactor" />
				<ref bean="LeaveAccrualCategoryRule-maxTransferAmount" />
				<ref bean="LeaveAccrualCategoryRule-maxPayoutAmount" />
				<ref bean="LeaveAccrualCategoryRule-maxPayoutLeaveCode" />
				<ref bean="LeaveAccrualCategoryRule-maxUsage" />
				<ref bean="LeaveAccrualCategoryRule-maxCarryOver" />
				<ref bean="LeaveAccrualCategoryRule-active" />
			</list>
		</property>
		<property name="relationships">
			<list>
				<bean parent="RelationshipDefinition">
					<property name="objectAttributeName" value="accrualCategoryObj" />
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition" p:sourceName="maxBalanceTransferToAccrualCategory"
								p:targetName="accrualCategory" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	<bean id="LeaveAccrualCategoryRule-serviceUnitOfTime" parent="LeaveAccrualCategoryRule-serviceUnitOfTime-parentBean" />
	<bean id="LeaveAccrualCategoryRule-serviceUnitOfTime-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Service Unit Of Time" />
		<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.lm.util.ServiceUnitOfTimeKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="serviceUnitOfTime" />
		<property name="label" value="Service Unit Of Time" />
	</bean>


  <bean id="LeaveAccrualCategoryRule-start" parent="LeaveAccrualCategoryRule-start-parentBean"/>
  <bean id="LeaveAccrualCategoryRule-start-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Start"/>
    <property name="maxLength" value="10"/>
   	<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
	</property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>    
    <property name="name" value="start"/>
    <property name="label" value="Start"/>
  </bean>
  
    <bean id="LeaveAccrualCategoryRule-end" parent="LeaveAccrualCategoryRule-end-parentBean"/>
  <bean id="LeaveAccrualCategoryRule-end-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="End"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
		<bean parent="NumericValidationPattern" />
	</property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>    
    <property name="name" value="end"/>
    <property name="label" value="End"/>
  </bean>
  
  
	<bean id="LeaveAccrualCategoryRule-accrualRate" parent="LeaveAccrualCategoryRule-accrualRate-parentBean" />
	<bean id="LeaveAccrualCategoryRule-accrualRate-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Accrual Rate" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="RegexValidationPattern">
 		   		<property name="pattern" value="((\+|-)?([0-9]{1,6}(\.[0-9]{1,2})?))()"></property>
 		   		<property name="validationErrorMessageKey" value="error.bigDecimal"></property>
 			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="accrualRate" />
		<property name="label" value="Accrual Rate" />
	</bean>
  
	<bean id="LeaveAccrualCategoryRule-maxBalance" parent="LeaveAccrualCategoryRule-maxBalance-parentBean" />
	<bean id="LeaveAccrualCategoryRule-maxBalance-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Max Balance" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="RegexValidationPattern">
 		   		<property name="pattern" value="((\+|-)?([0-9]{1,6}(\.[0-9]{1,2})?))()"></property>
 		   		<property name="validationErrorMessageKey" value="error.bigDecimal"></property>
 			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="maxBalance" />
		<property name="label" value="Max Balance" />
	</bean>

	<bean id="LeaveAccrualCategoryRule-maxBalanceActionFrequency" parent="LeaveAccrualCategoryRule-maxBalanceActionFrequency-parentBean" />
	<bean id="LeaveAccrualCategoryRule-maxBalanceActionFrequency-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Max Balance Action Frequency" />
		<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.lm.util.MaxBalanceActionFrequencyKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="maxBalanceActionFrequency" />
		<property name="label" value="Max Balance Action Frequency" />
	</bean>

	<bean id="LeaveAccrualCategoryRule-actionAtMaxBalance" parent="LeaveAccrualCategoryRule-actionAtMaxBalance-parentBean" />
	<bean id="LeaveAccrualCategoryRule-actionAtMaxBalance-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Action at Max Balance" />
		<property name="control">
			<bean parent="RadioControlDefinition" p:valuesFinderClass="org.kuali.hr.lm.util.MaxBalanceActionKeyValueFinder">
			</bean>
		</property>
		<property name="name" value="actionAtMaxBalance" />
		<property name="label" value="Action at Max Balance" />
	</bean>

	<bean id="LeaveAccrualCategoryRule-maxBalanceTransferToAccrualCategory" parent="LeaveAccrualCategoryRule-maxBalanceTransferToAccrualCategory-parentBean" />
	<bean id="LeaveAccrualCategoryRule-maxBalanceTransferToAccrualCategory-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Max Balance Transfer to AccrualCategory" />
		<property name="maxLength" value="15" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern">
				<property name="allowWhitespace" value="false" />
			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="maxBalanceTransferToAccrualCategory" />
		<property name="label" value="Max Balance Transfer to AccrualCategory" />
	</bean>

	<bean id="LeaveAccrualCategoryRule-maxBalanceTransferConversionFactor" parent="LeaveAccrualCategoryRule-maxBalanceTransferConversionFactor-parentBean" />
	<bean id="LeaveAccrualCategoryRule-maxBalanceTransferConversionFactor-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Max Balance Transfer ConversionFactor" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="RegexValidationPattern">
 		   		<property name="pattern" value="((\+|-)?([0-9]{1,6}(\.[0-9]{1,2})?))()"></property>
 		   		<property name="validationErrorMessageKey" value="error.bigDecimal"></property>
 			</bean>
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="maxBalanceTransferConversionFactor" />
		<property name="label" value="Max Balance Transfer ConversionFactor" />
	</bean>

	<bean id="LeaveAccrualCategoryRule-maxTransferAmount" parent="LeaveAccrualCategoryRule-maxTransferAmount-parentBean" />
	<bean id="LeaveAccrualCategoryRule-maxTransferAmount-parentBean" abstract="true"
