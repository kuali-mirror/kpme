	</bean>
	
	<!--  Max Usage -->
	<bean id="LeaveAccrualCategoryRule-maxCarryOver" parent="LeaveAccrualCategoryRule-maxCarryOver-parentBean" />
	<bean id="LeaveAccrualCategoryRule-maxCarryOver-parentBean" abstract="true"
