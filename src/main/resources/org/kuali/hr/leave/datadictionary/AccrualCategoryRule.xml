	<!--  Max Usage -->
	<bean id="LeaveAccrualCategoryRule-maxUsage" parent="LeaveAccrualCategoryRule-maxUsage-parentBean" />
	<bean id="LeaveAccrualCategoryRule-maxUsage-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Max Usage" />
		<property name="maxLength" value="10" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="10" />
		</property>
		<property name="name" value="maxUsage" />
