	<bean id="LeaveAccrualCategoryRule-active" parent="LeaveAccrualCategoryRule-active-parentBean" />
	<bean id="LeaveAccrualCategoryRule-active-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="shortLabel" value="Active" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="name" value="active" />
		<property name="label" value="Active" />
	</bean>

	<!--  Inquiry Definitions  -->
	<bean id="LeaveAccrualCategoryRule-lookupDefinition" parent="LeaveAccrualCategoryRule-lookupDefinition-parentBean" />
	<bean id="LeaveAccrualCategoryRule-lookupDefinition-parentBean" abstract="true"
		parent="LookupDefinition">
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="serviceUnitOfTime" />
				<bean parent="FieldDefinition" p:attributeName="start" />
				<bean parent="FieldDefinition" p:attributeName="end" />
				<bean parent="FieldDefinition" p:attributeName="accrualRate" />
				<bean parent="FieldDefinition" p:attributeName="maxBalance" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="serviceUnitOfTime" />
				<bean parent="FieldDefinition" p:attributeName="start" />
				<bean parent="FieldDefinition" p:attributeName="end" />
				<bean parent="FieldDefinition" p:attributeName="accrualRate" />
				<bean parent="FieldDefinition" p:attributeName="maxBalance" />
			</list>
		</property>
		<property name="title" value="LeaveAccrualCategoryRule Lookup" />
	</bean>

</beans>
