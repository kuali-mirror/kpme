		<field-descriptor name="timestamp"       column="timestamp" jdbc-type="TIMESTAMP" nullable="false" primarykey="false" />
		<field-descriptor name="objectId"        column="obj_id" jdbc-type="VARCHAR" index="true" />
		<field-descriptor name="versionNumber"   column="ver_nbr" jdbc-type="BIGINT" locking="true" />
	</class-descriptor>

	<!--  Added for Work Area Table -->

	<class-descriptor class="org.kuali.hr.time.workarea.WorkArea" table="tk_work_area_t">
		<field-descriptor name="workAreaId"         column="WORK_AREA_ID" jdbc-type="BIGINT" primarykey="true" autoincrement="true" sequence-name="tk_dept_lunch_rl_s"/>
		<!-- put field descriptors -->
		<field-descriptor name="effectiveDate"      column="effdt" jdbc-type="DATE" nullable="false" primarykey="false"/>
		<field-descriptor name="active"             column="active"	jdbc-type="BIT" length="1" nullable="false"	conversion="org.apache.ojb.broker.accesslayer.conversions.Boolean2IntFieldConversion"/>
		<field-descriptor name="description"        column="descr" jdbc-type="VARCHAR" length="30" nullable="false" primarykey="false" />
		<field-descriptor name="deptId"             column="dept_id" jdbc-type="VARCHAR" length="9" nullable="false" primarykey="false" />
		<field-descriptor name="emplType"           column="empl_type" jdbc-type="VARCHAR"  length="5" nullable="false" primarykey="false" />
		<field-descriptor name="overtimePreference" column="overtime_preference" jdbc-type="VARCHAR"  length="3" nullable="false" primarykey="false" />
		<field-descriptor name="adminDescr"         column="admin_descr"	jdbc-type="VARCHAR" length="30" nullable="false" />
		<field-descriptor name="userPrincipalId"    column="user_principal_id"	jdbc-type="VARCHAR" length="10" nullable="false" />

		<field-descriptor name="timestamp"          column="timestamp" jdbc-type="TIMESTAMP" nullable="false" primarykey="false" />
		<field-descriptor name="objectId"           column="obj_id" jdbc-type="VARCHAR" index="true" />
		<field-descriptor name="versionNumber"      column="ver_nbr" jdbc-type="BIGINT" locking="true" />
	</class-descriptor>

