<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" 
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="1" author="yingzhou" context="bootstrap">
        <comment>Renamed TKIPAddress to ClockLocationRuleIpAddress and made db changes</comment>
        <createTable tableName="tk_clock_loc_rl_ip_addr_s">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true"/>
            </column>
        </createTable>
		<modifySql dbms="mysql">
			<append value=" ENGINE MYISAM"/>
		</modifySql>
    </changeSet>
    <changeSet id="2" author="yingzhou" context="bootstrap">
        <comment>Renamed TKIPAddress to ClockLocationRuleIpAddress and made db changes</comment>
        <createTable tableName="tk_clock_loc_rl_ip_addr_t">
        	<column name="TK_CLOCK_LOC_RULE_IP_ID" type="VARCHAR(60)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
