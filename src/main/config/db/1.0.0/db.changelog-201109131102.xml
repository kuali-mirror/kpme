            <column name="ovt_pref" type="VARCHAR(5)" />
        </addColumn>
    </changeSet>
    <changeSet id="2" author="lee55" context="kpme-client-bootstrap">
        <comment>more sql changes</comment>
		<modifyDataType tableName="tk_time_block_t" columnName="CLOCK_LOG_CREATED" newDataType="VARCHAR(1)"/>
        <modifyDataType tableName="tk_time_block_hist_t" columnName="CLOCK_LOG_CREATED" newDataType="VARCHAR(1)"/>
	</changeSet>

</databaseChangeLog>