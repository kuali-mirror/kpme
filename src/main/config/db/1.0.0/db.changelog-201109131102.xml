<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" 
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="1" author="lee55" context="kpme-client-bootstrap">
        <comment>KPME-744. Timesheet - Allow users to change overtime earncode to new value (exmp. CPE)</comment>
        <addColumn tableName="TK_TIME_BLOCK_T">
            <column name="OVT_PREF" type="VARCHAR(5)" />
        </addColumn>
    </changeSet>
    <changeSet id="2" author="lee55" context="kpme-client-bootstrap">
        <comment>more sql changes</comment>
        <dropDefaultValue tableName="TK_TIME_BLOCK_T" columnName="CLOCK_LOG_CREATED" />
		<modifyDataType tableName="TK_TIME_BLOCK_T" columnName="CLOCK_LOG_CREATED" newDataType="VARCHAR(1)" />
        <addDefaultValue tableName="TK_TIME_BLOCK_T" columnName="CLOCK_LOG_CREATED" defaultValue="N" />
        <dropDefaultValue tableName="TK_TIME_BLOCK_HIST_T" columnName="CLOCK_LOG_CREATED" />
        <modifyDataType tableName="TK_TIME_BLOCK_HIST_T" columnName="CLOCK_LOG_CREATED" newDataType="VARCHAR(1)" />
        <addDefaultValue tableName="TK_TIME_BLOCK_HIST_T" columnName="CLOCK_LOG_CREATED" defaultValue="N" />
	</changeSet>

</databaseChangeLog>