<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">


    <bean id="EDOReviewLayerDefinition" parent="EDOReviewLayerDefinition-parentBean"/>
    <bean id="EDOReviewLayerDefinition-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.kpme.edo.reviewlayerdef.EdoReviewLayerDefinition"/>
        <property name="titleAttribute" value="reviewLayerDefinitionId"/>
        <property name="objectLabel" value="EDOReviewLayerDefinition"/>
        <property name="lookupDefinition">
            <ref bean="EDOReviewLayerDefinition-lookupDefinition"/>
        </property>
        <property name="inquiryDefinition">
            <ref bean="EDOReviewLayerDefinition-inquiryDefinition" />
        </property>
        <property name="attributes">
            <list>
                <ref bean="EDOReviewLayerDefinition-nodeName"/>
                <ref bean="EDOReviewLayerDefinition-voteType"/>
                <ref bean="EDOReviewLayerDefinition-description"/>
                <ref bean="EDOReviewLayerDefinition-reviewLetter"/>
                <ref bean="EDOReviewLayerDefinition-routeLevel"/>
                <ref bean="EDOReviewLayerDefinition-reviewLevel"/>
               <!--  <ref bean="EDOReviewLayerDefinition-suppNotify"/>
                <ref bean="EDOReviewLayerDefinition-suppNodeName"/> -->
                <ref bean="EDOReviewLayerDefinition-createDate"/>
                <ref bean="EDOReviewLayerDefinition-createdBy"/>
                <ref bean="EDOReviewLayerDefinition-lastUpdated"/>
                <ref bean="EDOReviewLayerDefinition-updatedBy"/>
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->

    <bean id="EDOReviewLayerDefinition-nodeName" parent="EDOReviewLayerDefinition-nodeName-parentBean"/>
    <bean id="EDOReviewLayerDefinition-nodeName-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Node Name"/>
        <property name="maxLength" value="30"/>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="30"/>
        </property>
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
                <property name="allowWhitespace" value="false" />
            </bean>
        </property>
        <property name="name" value="nodeName"/>
        <property name="label" value="Node Name"/>
    </bean>

    <bean id="EDOReviewLayerDefinition-voteType" parent="EDOReviewLayerDefinition-voteType-parentBean" />
    <bean id="EDOReviewLayerDefinition-voteType-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="shortLabel" value="Vote Type" />
        <property name="maxLength" value="16" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
                <property name="allowWhitespace" value="false" />
            </bean>
        </property>
        <property name="control">
            <bean parent="SelectControlDefinition"
                  p:valuesFinderClass="org.kuali.kpme.edo.vote.type.EdoVoteTypeValuesFinder"
                  p:includeKeyInLabel="false" />
        </property>
        <property name="name" value="voteType" />
        <property name="label" value="Vote Type" />
    </bean>

    <bean id="EDOReviewLayerDefinition-description" parent="EDOReviewLayerDefinition-description-parentBean"/>
    <bean id="EDOReviewLayerDefinition-description-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Description"/>
        <property name="maxLength" value="120"/>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="30" />
        </property>
        <property name="name" value="description"/>
        <property name="label" value="Description"/>
    </bean>

    <bean id="EDOReviewLayerDefinition-reviewLetter" parent="EDOReviewLayerDefinition-reviewLetter-parentBean" />
    <bean id="EDOReviewLayerDefinition-reviewLetter-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="shortLabel" value="Review Letter" />
        <property name="control">
            <bean parent="CheckboxControlDefinition" />
        </property>
        <property name="name" value="reviewLetter" />
        <property name="label" value="Review Letter" />
        <property name="formatterClass"
                  value="org.kuali.rice.core.web.format.BooleanFormatter" />
    </bean>

    <bean id="EDOReviewLayerDefinition-reviewLevel" parent="EDOReviewLayerDefinition-reviewLevel-parentBean"/>
    <bean id="EDOReviewLayerDefinition-reviewLevel-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Review Level" />
        <property name="maxLength" value="3" />
        <property name="validationPattern">
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="3" />
        </property>
        <property name="name" value="reviewLevel" />
        <property name="label" value="Review Level" />
    </bean>

    <bean id="EDOReviewLayerDefinition-routeLevel" parent="EDOReviewLayerDefinition-routeLevel-parentBean"/>
    <bean id="EDOReviewLayerDefinition-routeLevel-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Route Level" />
        <property name="maxLength" value="3" />
        <property name="validationPattern">
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="3" />
        </property>
        <property name="name" value="routeLevel" />
        <property name="label" value="Route Level" />
    </bean>

   <!--  <bean id="EDOReviewLayerDefinition-suppNotify" parent="EDOReviewLayerDefinition-suppNotify-parentBean" />
    <bean id="EDOReviewLayerDefinition-suppNotify-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="shortLabel" value="Supp. Notification" />
        <property name="control">
            <bean parent="CheckboxControlDefinition" />
        </property>
        <property name="name" value="suppNotify" />
        <property name="label" value="Supplemental Materials Notification" />
        <property name="formatterClass"
                  value="org.kuali.rice.core.web.format.BooleanFormatter" />
    </bean>

    <bean id="EDOReviewLayerDefinition-suppNodeName" parent="EDOReviewLayerDefinition-suppNodeName-parentBean"/>
    <bean id="EDOReviewLayerDefinition-suppNodeName-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Supp. Node Name"/>
        <property name="maxLength" value="30"/>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="30"/>
        </property>
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
                <property name="allowWhitespace" value="false" />
            </bean>
        </property>
        <property name="name" value="suppNodeName"/>
        <property name="label" value="Supplemental Node Name"/>
    </bean> -->

    <bean id="EDOReviewLayerDefinition-createDate" parent="EDOReviewLayerDefinition-createDate-parentBean" />
    <bean id="EDOReviewLayerDefinition-createDate-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="shortLabel" value="Created Date" />
        <property name="maxLength" value="10" />
        <property name="validationPattern">
            <bean parent="DateValidationPattern" />
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
        </property>
        <property name="formatterClass"
                  value="org.kuali.rice.core.web.format.DateFormatter" />
        <property name="name" value="createDate" />
        <property name="label" value="Created Date" />
    </bean>

    <bean id="EDOReviewLayerDefinition-createdBy" parent="EDOReviewLayerDefinition-createdBy-parentBean" />
    <bean id="EDOReviewLayerDefinition-createdBy-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="shortLabel" value="Created By" />
        <property name="maxLength" value="16" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
                <property name="allowWhitespace" value="false" />
            </bean>
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="16" />
        </property>
        <property name="name" value="createdBy" />
        <property name="label" value="Created By" />
    </bean>

    <bean id="EDOReviewLayerDefinition-lastUpdated" parent="EDOReviewLayerDefinition-lastUpdated-parentBean" />
    <bean id="EDOReviewLayerDefinition-lastUpdated-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="shortLabel" value="Last Updated" />
        <property name="maxLength" value="10" />
        <property name="validationPattern">
            <bean parent="DateValidationPattern" />
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
        </property>
        <property name="formatterClass"
                  value="org.kuali.rice.core.web.format.DateFormatter" />
        <property name="name" value="lastUpdated" />
        <property name="label" value="Last Updated" />
    </bean>

    <bean id="EDOReviewLayerDefinition-updatedBy" parent="EDOReviewLayerDefinition-updatedBy-parentBean" />
    <bean id="EDOReviewLayerDefinition-updatedBy-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="shortLabel" value="Updated By" />
        <property name="maxLength" value="16" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
                <property name="allowWhitespace" value="false" />
            </bean>
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="16" />
        </property>
        <property name="name" value="updatedBy" />
        <property name="label" value="Updated By" />
    </bean>

    <!-- -->
    <!-- Lookup Definition: -->
    <!-- -->

    <bean id="EDOReviewLayerDefinition-lookupDefinition" parent="EDOReviewLayerDefinition-lookupDefinition-parentBean"/>
    <bean id="EDOReviewLayerDefinition-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="lookupableID" value="edoReviewLayerDefinitionLookupable" />
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="nodeName"/>
                <bean parent="FieldDefinition" p:attributeName="voteType"/>
                <bean parent="FieldDefinition" p:attributeName="reviewLetter"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="nodeName"/>
                <bean parent="FieldDefinition" p:attributeName="voteType"/>
                <bean parent="FieldDefinition" p:attributeName="description"/>
                <bean parent="FieldDefinition" p:attributeName="reviewLetter"/>
                <bean parent="FieldDefinition" p:attributeName="reviewLevel"/>
                <bean parent="FieldDefinition" p:attributeName="routeLevel"/>
                <!-- <bean parent="FieldDefinition" p:attributeName="suppNotify"/>
                <bean parent="FieldDefinition" p:attributeName="suppNodeName"/> -->
                <bean parent="FieldDefinition" p:attributeName="lastUpdated"/>
                <bean parent="FieldDefinition" p:attributeName="updatedBy"/>
            </list>
        </property>
        <property name="title" value="Review Layer Definition Lookup"/>
    </bean>

    <!-- -->
    <!-- Inquiry Definition: -->
    <!-- -->
    <bean id="EDOReviewLayerDefinition-inquiryDefinition" parent="EDOReviewLayerDefinition-inquiryDefinition-parentBean" />
    <bean id="EDOReviewLayerDefinition-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Review Layer Defnition Inquiry"/>
        <property name="inquirySections">
            <list>
                <bean parent="InquirySectionDefinition" p:title="Review Layer Definition Details">
                    <property name="inquiryFields">
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="nodeName"/>
                            <bean parent="FieldDefinition" p:attributeName="voteType"/>
                            <bean parent="FieldDefinition" p:attributeName="description"/>
                            <bean parent="FieldDefinition" p:attributeName="reviewLetter"/>
                            <bean parent="FieldDefinition" p:attributeName="reviewLevel"/>
                            <bean parent="FieldDefinition" p:attributeName="routeLevel"/>
                            <!-- <bean parent="FieldDefinition" p:attributeName="suppNotify"/>
                            <bean parent="FieldDefinition" p:attributeName="suppNodeName"/> -->
                            <bean parent="FieldDefinition" p:attributeName="createDate"/>
                            <bean parent="FieldDefinition" p:attributeName="createdBy"/>
                            <bean parent="FieldDefinition" p:attributeName="lastUpdated"/>
                            <bean parent="FieldDefinition" p:attributeName="updatedBy"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
        <property name="translateCodes" value="false"/>
    </bean>

</beans>
