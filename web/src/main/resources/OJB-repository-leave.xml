        <field-descriptor name="lmSystemScheduledTimeOffId"   column="LM_SYS_SCHD_TIMEOFF_ID" jdbc-type="VARCHAR"     length="60" primarykey="true" autoincrement="true" sequence-name="LM_SYS_SCHD_TIMEOFF_S" />
