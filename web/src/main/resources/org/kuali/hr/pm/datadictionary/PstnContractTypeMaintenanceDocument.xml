  <bean id="PstnContractTypeMaintenanceDocument" parent="PstnContractTypeMaintenanceDocument-parentBean"/>
  <bean id="PstnContractTypeMaintenanceDocument-parentBean" abstract="true" parent="uifMaintenanceDocumentEntry">
