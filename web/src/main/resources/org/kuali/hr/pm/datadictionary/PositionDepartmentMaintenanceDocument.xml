<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:dd="http://rice.kuali.org/dd"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
                           http://rice.kuali.org/dd
                           http://rice.kuali.org/dd/dd.xsd">

  <bean id="PositionDepartmentMaintenanceDocument" parent="PositionDepartmentMaintenanceDocument-parentBean"/>
  <bean id="PositionDepartmentMaintenanceDocument-parentBean" abstract="true" parent="uifMaintenanceDocumentEntry">
    <property name="dataObjectClass" value="org.kuali.kpme.pm.positiondepartment.PositionDepartment"/>
    <property name="maintainableClass" value="org.kuali.kpme.pm.positiondepartment.web.PositionDepartmentMaintainableImpl"/>  
    <property name="documentTypeName" value="PositionDepartmentMaintenanceDocumentType"/>
    <property name="businessRulesClass" value="org.kuali.kpme.pm.positiondepartment.validation.PositionDepartmentValidation"/>
  </bean>
</beans>
