       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean id="PositionResponsibilityMaintenanceDocument" parent="PositionResponsibilityMaintenanceDocument-parentBean"/>
  <bean id="PositionResponsibilityMaintenanceDocument-parentBean" abstract="true" parent="uifMaintenanceDocumentEntry">
    <property name="dataObjectClass" value="org.kuali.kpme.pm.positionresponsibility.PositionResponsibility"/>  
    <property name="documentTypeName" value="ClassificationMaintenanceDocumentType" />
    <property name="maintainableClass" value="org.kuali.kpme.pm.classification.web.ClassificationMaintainableImpl"/>
  </bean>
  
</beans>
