<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="PositionFlag" parent="PositionFlag-parentBean"/>
 	<bean id="PositionFlag-parentBean" abstract="true" parent="BusinessObjectEntry">
	    <property name="dataObjectClass" value="org.kuali.kpme.pm.position.PstnFlag"/>
	    <property name="attributes">
	      <list>
	        <ref bean="PositionFlag-pmFlagId"/>
	        <ref bean="PositionFlag-category"/>
	        <ref bean="PositionFlag-names"/>
	        <ref bean="PositionFlag-hrPositionId"/>
	      </list>
	    </property>
	    <property name="objectLabel" value="Position Duty"/>
  	</bean>

<!-- Attribute Definitions -->  
	 <bean id="PositionFlag-pmFlagId" parent="PositionFlag-pmFlagId-parentBean"/>
	 <bean id="PositionFlag-pmFlagId-parentBean" abstract="true" parent="AttributeDefinition">
	    <property name="required" value="false" />
	    <property name="forceUppercase" value="false"/>
	    <property name="shortLabel" value="id"/>
	    <property name="controlField">
	      <bean parent="Uif-TextControl" p:size="60"/>
	    </property>
	    <property name="name" value="pmFlagId"/>
	    <property name="label" value="Position Flag Id"/>
	 </bean> 
	 
	<bean id="PositionFlag-category" parent="PositionFlag-category-parentBean" />
	<bean id="PositionFlag-category-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="required" value="true" />
		<property name="maxLength" value="20"/>
		<property name="controlField">
			<bean parent="Uif-TextAreaControl" />
		</property>
		<property name="name" value="category" />
		<property name="label" value="Category" />
		<property name="description" value="Category" />
	</bean>	
	 
	<bean id="PositionFlag-names" parent="PositionFlag-names-parentBean" />
	<bean id="PositionFlag-names-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="required" value="true" />
		<property name="maxLength" value="100"/>
		<property name="controlField">
			<bean parent="Uif-TextControl" p:size="20"/>
		</property>
		<property name="name" value="names" />
		<property name="label" value="Names" />
		<property name="description" value="Names" />
	</bean>	
	
 	<bean id="PositionFlag-hrPositionId" parent="PositionFlag-hrPositionId-parentBean"/>
	<bean id="PositionFlag-hrPositionId-parentBean" abstract="true" parent="AttributeDefinition">
	    <property name="required" value="false" />
	    <property name="forceUppercase" value="false"/>
	    <property name="shortLabel" value="id"/>
	    <property name="controlField">
	      <bean parent="Uif-TextControl" p:size="60"/>
	    </property>
	    <property name="name" value="hrPositionId"/>
	    <property name="label" value="Position Id"/>
	</bean> 
	
</beans>
