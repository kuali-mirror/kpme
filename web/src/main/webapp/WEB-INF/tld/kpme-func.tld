        <function-class>org.kuali.kpme.tklm.common.WorkflowTagSupport</function-class>
        <function-signature>java.lang.Boolean isTimesheetApprovalButtonsDisplaying(java.lang.String)</function-signature>
    </function>
    
    <function>
        <description>Determines if a Leave Calendar's approve button should be displayed</description>
        <name>isLeaveCalendarApprovalButtonsDisplaying</name>
        <function-class>org.kuali.kpme.tklm.common.WorkflowTagSupport</function-class>
        <function-signature>java.lang.Boolean isLeaveCalendarApprovalButtonsDisplaying(java.lang.String)</function-signature>
    </function>
    
    <function>
        <description>Determines if a Timesheet's approve button should be enabled</description>
        <name>isTimesheetApprovalButtonsEnabled</name>
        <function-class>org.kuali.kpme.tklm.common.WorkflowTagSupport</function-class>
        <function-signature>java.lang.Boolean isTimesheetApprovalButtonsEnabled(java.lang.String)</function-signature>
