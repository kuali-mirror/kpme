<taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd" version="2.1">

    <description>KPME functions library</description>
    <display-name>KPME functions</display-name>
    <tlib-version>1.0</tlib-version>
    <short-name>kpmefunc</short-name>

    <function>
        <description>Determines if a Timesheet's route button should be displayed</description>
        <name>isTimesheetRouteButtonDisplaying</name>
        <function-class>org.kuali.kpme.tklm.common.WorkflowTagSupport</function-class>
        <function-signature>java.lang.Boolean isTimesheetRouteButtonDisplaying(java.lang.String)</function-signature>
    </function>
    
    <function>
        <description>Determines if a Leave Calendar's route button should be displayed</description>
        <name>isLeaveCalendarRouteButtonDisplaying</name>
        <function-class>org.kuali.kpme.tklm.common.WorkflowTagSupport</function-class>
        <function-signature>java.lang.Boolean isLeaveCalendarRouteButtonDisplaying(java.lang.String)</function-signature>
    </function>
    
    <function>
        <description>Determines if a Timesheet's route button should be enabled</description>
        <name>isTimesheetRouteButtonEnabled</name>
        <function-class>org.kuali.kpme.tklm.common.WorkflowTagSupport</function-class>
        <function-signature>java.lang.Boolean isTimesheetRouteButtonEnabled(java.lang.String)</function-signature>
    </function>
    
    <function>
        <description>Determines if a Leave Calendar's route button should be enabled</description>
        <name>isLeaveCalendarRouteButtonEnabled</name>
        <function-class>org.kuali.kpme.tklm.common.WorkflowTagSupport</function-class>
        <function-signature>java.lang.Boolean isLeaveCalendarRouteButtonEnabled(java.lang.String)</function-signature>
    </function>
    
    <function>
        <description>Determines if a Timesheet's approve button should be displayed</description>
